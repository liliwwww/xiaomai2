<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#f4f5f6"/>
        </shape>
    </item>
    <item
        android:height="60dp"
        android:top="0dp">
