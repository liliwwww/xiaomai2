<?xml version="1.0" encoding="utf-8"?>
<com.alibaba.pictures.bricks.view.ResizableRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:clipChildren="true"
    android:clipToPadding="true"
    android:layout_width="93dp"
    android:layout_height="235dp"
    android:paddingStart="@dimen/resource_size_8"
    android:layout_marginEnd="-2dp">
    <RelativeLayout
        android:id="@+id/project_top_lay"
        android:background="@drawable/onearch_white_corner_6"
        android:padding="@dimen/resource_size_4"
        android:layout_width="match_parent"
        android:layout_height="@dimen/resource_size_111"
        android:layout_above="@+id/project_bottom_lay">
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <com.alibaba.pictures.bricks.view.RoundImageView
                android:layout_gravity="center"
                android:id="@+id/blur_bg_static"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/bricks_blur_bg_static"
                android:scaleType="fitXY"
                app:borderRadius="6dp"
                app:type="radial"/>
            <View
                android:layout_gravity="center"
                android:id="@+id/blur_bg"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>
        </FrameLayout>
        <FrameLayout
            android:layout_width="77dp"
            android:layout_height="103dp"
            android:layout_centerInParent="true">
            <com.alibaba.pictures.bricks.view.RoundImageView
                android:id="@+id/homepage_item_image_normal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/bricks_uikit_default_image_bg_gradient"
                android:scaleType="centerCrop"
                app:borderRadius="6dp"
                app:type="radial"/>
            <com.alibaba.pictures.bricks.view.RoundImageView
                android:id="@+id/homepage_item_image_border"
                android:background="@drawable/bricks_homepage_project_image_border"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="centerCrop"
                app:borderRadius="6dp"
                app:type="radial"/>
            <FrameLayout
                android:layout_gravity="bottom"
                android:id="@+id/homepage_item_image_normal_cover"
                android:background="@drawable/bricks_home_projectitem_cover"
                android:layout_width="match_parent"
                android:layout_height="75.5dp"/>
            <com.alibaba.pictures.bricks.view.DMDigitTextView
                android:textSize="20dp"
                android:textColor="@color/white"
                android:gravity="center"
                android:id="@+id/tv_order"
                android:background="@drawable/bg_rank_project_item_order1"
                android:paddingBottom="3dp"
                android:visibility="gone"
                android:layout_width="@dimen/margin_23dp"
                android:layout_height="@dimen/margin_30dp"
                android:layout_marginStart="6dp"/>
            <com.alibaba.pictures.bricks.view.DMCategroyTagView
                android:layout_gravity="right"
                android:id="@+id/homepage_item_image_tag"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="3dp"/>
            <com.alibaba.pictures.bricks.view.DMDigitTextView
                android:textSize="10dp"
                android:textColor="#ffffff"
                android:ellipsize="end"
                android:gravity="bottom"
                android:layout_gravity="bottom"
                android:id="@+id/homepage_item_bottom_left"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginBottom="3dp"
                android:text="11.20开播"
                android:maxLines="1"/>
        </FrameLayout>
    </RelativeLayout>
    <com.alibaba.pictures.bricks.view.SafeLottieAnimationView
        android:id="@+id/homepage_item_price_preferential_lottie_big"
        android:visibility="gone"
        android:layout_width="74dp"
        android:layout_height="29dp"
        android:layout_marginBottom="17dp"
        android:layout_alignParentBottom="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="5dp"
        app:lottie_autoPlay="false"/>
    <LinearLayout
        android:layout_gravity="center"
        android:orientation="vertical"
        android:id="@+id/project_bottom_lay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:paddingStart="@dimen/resource_size_2">
        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="@dimen/resource_size_35"
            android:layout_marginTop="5dp">
            <com.alibaba.pictures.bricks.view.FakeBoldTextView
                android:textSize="@dimen/font_Subhead2_M"
                android:textColor="@color/cg_1"
                android:ellipsize="end"
                android:id="@+id/homepage_project_item_title_wide"
                android:visibility="gone"
                android:layout_width="138dp"
                android:layout_height="match_parent"
                android:maxLines="2"
                android:minLines="2"
                android:lineSpacingExtra="2dp"/>
            <com.alibaba.pictures.bricks.view.FakeBoldTextView
                android:textSize="@dimen/font_Subhead2_M"
                android:textColor="@color/cg_1"
                android:ellipsize="end"
                android:id="@+id/homepage_project_item_title"
                android:layout_width="85dp"
                android:layout_height="match_parent"
                android:maxLines="2"
                android:minLines="2"
                android:lineSpacingExtra="2dp"/>
        </FrameLayout>
        <LinearLayout
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="3dp">
            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:orientation="horizontal"
                    android:id="@+id/homepage_item_price_layout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <com.alibaba.pictures.bricks.view.DigitTextView
                        android:textSize="10dp"
                        android:textColor="#9c9ca5"
                        android:layout_gravity="bottom"
                        android:id="@+id/homepage_item_price_flag"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="¥"/>
                    <com.alibaba.pictures.bricks.view.DigitTextView
                        android:textSize="10dp"
                        android:textColor="#9c9ca5"
                        android:id="@+id/homepage_item_price"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="2dp"
                        android:singleLine="true"/>
                </LinearLayout>
                <View
                    android:layout_gravity="center_vertical"
                    android:id="@+id/homepage_item_price_line"
                    android:background="#809c9ca5"
                    android:layout_width="10dp"
                    android:layout_height="1dp"/>
            </FrameLayout>
            <TextView
                android:textSize="10dp"
                android:textColor="#ff4886"
                android:ellipsize="end"
                android:id="@+id/homepage_item_discountDesc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="0.5dp"
                android:maxLines="1"
                android:layout_marginStart="2dp"/>
        </LinearLayout>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:layout_marginBottom="10dp">
            <com.alibaba.pictures.bricks.view.DigitTextView
                android:textSize="10dp"
                android:textColor="#ff4886"
                android:id="@+id/homepage_item_price_preferential_flag"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="1dp"
                android:text="¥"
                android:layout_alignBaseline="@+id/homepage_item_price_preferential"/>
            <com.alibaba.pictures.bricks.view.DigitTextView
                android:textSize="15dp"
                android:textColor="#ff4886"
                android:ellipsize="end"
                android:id="@+id/homepage_item_price_preferential"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="1.5dp"
                android:maxLines="1"
                android:singleLine="true"
                android:layout_toRightOf="@+id/homepage_item_price_preferential_flag"
                android:layout_marginEnd="5dp"/>
            <com.alibaba.pictures.bricks.view.SafeLottieAnimationView
                android:id="@+id/homepage_item_price_preferential_lottie_little"
                android:visibility="visible"
                android:layout_width="15dp"
                android:layout_height="25dp"
                android:layout_marginBottom="4dp"
                android:layout_toRightOf="@+id/homepage_item_price_preferential"
                android:layout_marginStart="-7dp"
                app:lottie_autoPlay="false"/>
        </RelativeLayout>
    </LinearLayout>
    <ImageView
        android:id="@+id/project_flash_sale_icon"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/bricks_icon_coming_show_flash_sale"
        android:layout_alignParentEnd="true"/>
    <TextView
        android:textSize="11dp"
        android:textColor="@color/white"
        android:gravity="center"
        android:id="@+id/homepage_item_buy_tickets_btn"
        android:background="@drawable/bricks_coming_show_item_buy_tickets_bg"
        android:paddingBottom="1dp"
        android:visibility="gone"
        android:layout_width="51dp"
        android:layout_height="24dp"
        android:layout_marginBottom="16dp"
        android:text="购票"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="5dp"/>
</com.alibaba.pictures.bricks.view.ResizableRelativeLayout>
