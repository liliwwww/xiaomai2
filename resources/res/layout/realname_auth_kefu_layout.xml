<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@drawable/realname_auth_kefu_bg"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <cn.damai.uikit.iconfont.DMIconFontTextView
        android:textSize="16dp"
        android:textColor="@color/color_000000"
        android:layout_gravity="center_vertical"
        android:id="@+id/realname_right_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/margin_9dp"
        android:text="@string/iconfont_kefu_"/>
    <TextView
        android:textSize="@dimen/margin_12dp"
        android:textColor="@color/color_000000"
        android:layout_gravity="center_vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/margin_3dp"
        android:layout_marginRight="@dimen/margin_9dp"
        android:text="在线客服"/>
</LinearLayout>
