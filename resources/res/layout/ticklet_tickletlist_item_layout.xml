<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/ticklet_perform_item_root"
    android:paddingLeft="12dp"
    android:paddingRight="12dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <RelativeLayout
        android:id="@+id/ticklet_perform_item_view"
        android:background="@drawable/bg_border_ffffff_corner_12dp_ticket"
        android:layout_width="match_parent"
        android:layout_height="304dp"
        android:layout_marginTop="31dp"
        android:layout_marginStart="15dp">
        <com.alibaba.pictures.moimage.MoImageView
            android:layout_gravity="center"
            android:id="@+id/ticklet_perform_list_project_image"
            android:layout_width="165dp"
            android:layout_height="219dp"
            android:layout_marginTop="@dimen/margin_16dp"
            android:scaleType="centerCrop"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="@dimen/margin_16dp"
            app:roundedCornerRadius="@dimen/margin_6dp"
            app:roundingBorderColor="#0d000000"
            app:roundingBorderWidth="1dp"/>
        <ImageView
            android:id="@+id/ticklet_dm_bg_icon"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginTop="5.5dp"
            android:layout_marginRight="9dp"
            android:src="@drawable/ticklet_dm_bg_icon"
            android:scaleType="fitXY"
            android:layout_alignParentEnd="true"/>
        <TextView
            android:textSize="15dp"
            android:textColor="#2e333e"
            android:ellipsize="end"
            android:id="@+id/ticklet_list_performname"
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:layout_marginTop="9dp"
            android:layout_marginBottom="15dp"
            android:maxLines="3"
            android:includeFontPadding="false"
            android:layout_below="@+id/ticklet_perform_list_project_image"
            android:lineSpacingMultiplier="0.9"
            android:layout_alignStart="@+id/ticklet_perform_list_project_image"
            android:layout_alignEnd="@+id/ticklet_perform_list_project_image"/>
        <TextView
            android:textSize="11dp"
            android:textColor="#959aa5"
            android:ellipsize="end"
            android:id="@+id/perform_address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="19dp"
            android:text="场地"
            android:maxLines="1"
            android:layout_alignParentTop="true"
            android:layout_marginStart="32dp"
            android:layout_toEndOf="@+id/ticklet_perform_list_project_image"/>
        <TextView
            android:textSize="12dp"
            android:textColor="#2e333e"
            android:ellipsize="end"
            android:id="@+id/ticklet_list_performaddress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_6dp"
            android:layout_marginRight="@dimen/margin_14dp"
            android:maxLines="2"
            android:layout_below="@+id/perform_address"
            android:lineSpacingExtra="@dimen/margin_5dp"
            android:layout_marginEnd="16dp"
            android:layout_alignStart="@+id/perform_address"/>
        <TextView
            android:textSize="11dp"
            android:textColor="#959aa5"
            android:ellipsize="end"
            android:gravity="left"
            android:id="@+id/perform_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_18dp"
            android:text="时间"
            android:layout_below="@+id/ticklet_list_performaddress"
            android:lineSpacingExtra="@dimen/margin_3dp"
            android:layout_marginStart="32dp"
            android:layout_toEndOf="@+id/ticklet_perform_list_project_image"/>
        <ImageView
            android:gravity="center"
            android:id="@+id/perform_time_change_icon"
            android:layout_width="43.5dp"
            android:layout_height="17dp"
            android:src="@drawable/ticklet_perform_cancel"
            android:layout_alignTop="@+id/perform_time"
            android:layout_alignBottom="@+id/perform_time"
            android:layout_marginStart="5dp"
            android:layout_toEndOf="@+id/perform_time"/>
        <TextView
            android:textSize="13dp"
            android:textColor="#2e333e"
            android:ellipsize="end"
            android:gravity="top|left"
            android:id="@+id/ticklet_list_performtime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_6dp"
            android:maxLines="3"
            android:layout_below="@+id/perform_time"
            android:lineSpacingExtra="@dimen/margin_3dp"
            android:layout_marginEnd="16dp"
            android:layout_alignStart="@+id/perform_time"/>
        <ImageView
            android:id="@+id/ticklet_perform_item_status_icon"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginTop="132dp"
            android:src="@drawable/ticklet_perform_end_icon"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/ticklet_perform_list_project_image"
            android:layout_alignParentEnd="true"
            android:elevation="2dp"/>
        <LinearLayout
            android:gravity="center"
            android:orientation="vertical"
            android:id="@+id/ticklet_list_item_num_layout"
            android:background="@drawable/ticklet_circle_right_sharp_bg"
            android:paddingRight="@dimen/margin_1dp"
            android:paddingBottom="@dimen/margin_2dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minWidth="32dp"
            android:minHeight="32dp"
            android:layout_alignLeft="@+id/ticklet_perform_list_project_image"
            android:layout_alignBottom="@+id/ticklet_perform_list_project_image">
            <cn.damai.uikit.number.DMDigitTextView
                android:textSize="@dimen/margin_16dp"
                android:textColor="?attr/tickletListLeftBottomTextColor"
                android:id="@+id/ticklet_list_item_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="-3dp"/>
            <TextView
                android:textSize="@dimen/margin_8dp"
                android:textColor="?attr/tickletListLeftBottomTextColor"
                android:id="@+id/ticklet_list_item_num_unit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="张票"/>
        </LinearLayout>
        <LinearLayout
            android:gravity="center"
            android:orientation="vertical"
            android:id="@+id/ticklet_list_item_button_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20dp"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="30dp"
            android:layout_marginEnd="20dp"
            android:layout_toEndOf="@+id/ticklet_perform_list_project_image"
            android:elevation="2dp"/>
        <ImageView
            android:id="@+id/ticklet_perform_list_divider"
            android:layout_width="17dp"
            android:layout_height="match_parent"
            android:src="@drawable/ticklet_perform_sep_new"
            android:scaleType="fitCenter"
            android:layout_marginStart="8.5dp"
            android:layout_toEndOf="@+id/ticklet_perform_list_project_image"/>
    </RelativeLayout>
    <ImageView
        android:id="@+id/ticklet_damai_left_icon"
        android:layout_width="15dp"
        android:layout_height="303dp"
        android:layout_marginTop="31dp"
        android:src="@drawable/ticklet_damai_left_icon"
        android:scaleType="centerCrop"/>
    <cn.damai.ticklet.view.FrameLayoutRoundGradientView
        android:orientation="vertical"
        android:id="@+id/ticklet_recent_show_layout"
        android:layout_width="match_parent"
        android:layout_height="39dp">
        <ImageView
            android:layout_gravity="center_vertical"
            android:id="@+id/ticklet_recent_show_text"
            android:layout_width="wrap_content"
            android:layout_height="18dp"
            android:src="@drawable/ticklet_recent_show_bg"
            android:scaleType="fitStart"
            android:layout_marginStart="16.5dp"/>
    </cn.damai.ticklet.view.FrameLayoutRoundGradientView>
</FrameLayout>
