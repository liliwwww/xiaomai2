<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/ll_root"
    android:background="@drawable/order_result_item_background"
    android:paddingBottom="9dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="12dp"
    android:layout_marginRight="12dp">
    <TextView
        android:textSize="14dp"
        android:textColor="@color/color_tpp_primary_title"
        android:gravity="center"
        android:id="@+id/tv_top_tips"
        android:paddingTop="14.5dp"
        android:paddingBottom="14.5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:includeFontPadding="false"/>
    <LinearLayout
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp">
        <RelativeLayout
            android:id="@+id/rl_left_item"
            android:background="#fef9f1"
            android:paddingBottom="9dp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="5.5dp"
            android:layout_weight="1">
            <com.taobao.movie.android.commonui.widget.IconFontTextView
                android:textSize="50dp"
                android:textColor="@color/color_tpp_primary_gray_c8"
                android:id="@+id/if_already_choose"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="-11dp"
                android:text="@string/icon_font_youku_choose"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"/>
            <TextView
                android:textSize="10dp"
                android:textColor="@color/white"
                android:gravity="center"
                android:id="@+id/tv_top_notice"
                android:padding="2dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:includeFontPadding="false"/>
            <LinearLayout
                android:gravity="bottom"
                android:orientation="horizontal"
                android:id="@+id/rl_top_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/tv_top_notice"
                android:layout_centerHorizontal="true">
                <com.alibaba.pictures.moimage.MoImageView
                    android:id="@+id/img_icon"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_marginBottom="1dp"
                    app:roundAsCircle="true"/>
                <TextView
                    android:textSize="14dp"
                    android:textColor="@color/color_tpp_primary_main_title"
                    android:id="@+id/tv_youku_right"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1.5dp"
                    android:layout_toRightOf="@+id/img_icon"/>
            </LinearLayout>
            <LinearLayout
                android:gravity="bottom|center_horizontal"
                android:orientation="horizontal"
                android:id="@+id/rl_discount_price"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:layout_below="@+id/rl_top_title"
                android:layout_centerHorizontal="true">
                <TextView
                    android:textSize="11dp"
                    android:textColor="@color/tpp_primary_red"
                    android:id="@+id/tv_unit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="¥"/>
                <TextView
                    android:textSize="20.5dp"
                    android:textColor="@color/tpp_primary_red"
                    android:id="@+id/tv_cost"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
            <TextView
                android:textSize="11dp"
                android:textColor="@color/color_tpp_primary_assist"
                android:id="@+id/tv_price_normal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/rl_discount_price"
                android:layout_centerHorizontal="true"/>
            <TextView
                android:textSize="11dp"
                android:textColor="@color/tpp_primary_red"
                android:id="@+id/tv_price_save"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="1dp"
                android:layout_below="@+id/tv_price_normal"
                android:layout_centerHorizontal="true"/>
            <com.taobao.movie.android.commonui.widget.TimerTextView
                android:textSize="11dp"
                android:textColor="@color/white"
                android:ellipsize="end"
                android:gravity="center"
                android:id="@+id/order_time_text_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="12dp"
                android:layout_marginTop="9dp"
                android:layout_marginRight="12dp"
                android:text="选我"
                android:maxLines="1"
                android:includeFontPadding="false"
                android:layout_below="@+id/tv_price_save"
                style="@style/buy_button_style_small_7_3"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/rl_right_item"
            android:background="#fef9f1"
            android:paddingBottom="9dp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5.5dp"
            android:layout_weight="1">
            <com.taobao.movie.android.commonui.widget.IconFontTextView
                android:textSize="50dp"
                android:textColor="@color/color_tpp_primary_gray_c8"
                android:id="@+id/if_already_choose2"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="-11dp"
                android:text="@string/icon_font_youku_choose"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"/>
            <TextView
                android:textSize="10dp"
                android:textColor="@color/white"
                android:gravity="center"
                android:id="@+id/tv_top_notice2"
                android:padding="2dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:includeFontPadding="false"/>
            <LinearLayout
                android:gravity="bottom"
                android:orientation="horizontal"
                android:id="@+id/rl_top_title2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/tv_top_notice2"
                android:layout_centerHorizontal="true">
                <com.alibaba.pictures.moimage.MoImageView
                    android:id="@+id/img_icon2"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_marginBottom="1dp"
                    app:roundAsCircle="true"/>
                <TextView
                    android:textSize="14dp"
                    android:textColor="@color/color_tpp_primary_main_title"
                    android:id="@+id/tv_youku_right2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1.5dp"
                    android:layout_toRightOf="@+id/img_icon"/>
            </LinearLayout>
            <LinearLayout
                android:gravity="bottom|center_horizontal"
                android:orientation="horizontal"
                android:id="@+id/rl_discount_price2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:layout_below="@+id/rl_top_title2"
                android:layout_centerHorizontal="true">
                <TextView
                    android:textSize="11dp"
                    android:textColor="@color/tpp_primary_red"
                    android:id="@+id/tv_unit2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="¥"/>
                <TextView
                    android:textSize="20.5dp"
                    android:textColor="@color/tpp_primary_red"
                    android:id="@+id/tv_cost2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
            <TextView
                android:textSize="11dp"
                android:textColor="@color/color_tpp_primary_assist"
                android:id="@+id/tv_price_normal2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/rl_discount_price2"
                android:layout_centerHorizontal="true"/>
            <TextView
                android:textSize="11dp"
                android:textColor="@color/tpp_primary_red"
                android:id="@+id/tv_price_save2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="1dp"
                android:layout_below="@+id/tv_price_normal2"
                android:layout_centerHorizontal="true"/>
            <com.taobao.movie.android.commonui.widget.TimerTextView
                android:textSize="11dp"
                android:textColor="@color/white"
                android:ellipsize="end"
                android:gravity="center"
                android:id="@+id/order_time_text_view2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="12dp"
                android:layout_marginTop="9dp"
                android:layout_marginRight="12dp"
                android:text="选我"
                android:maxLines="1"
                android:includeFontPadding="false"
                android:layout_below="@+id/tv_price_save2"
                style="@style/buy_button_style_small_7_3"/>
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>
