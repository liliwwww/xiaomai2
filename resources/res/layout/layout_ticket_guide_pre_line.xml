<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include layout="@layout/layout_ticket_guide_pre_base"/>
    <cn.damai.uikit.view.DashedLine
        android:id="@+id/top_dash_line"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        app:dashColor="#dbdbdb"
        app:dashGap="2dp"
        app:dashOrientation="horizontal"
        app:dashWidth="2dp"
        app:strokeW="1dp"/>
    <cn.damai.uikit.view.DashedLine
        android:id="@+id/dash_vertical_view"
        android:layout_width="1dp"
        android:layout_height="154dp"
        app:dashColor="#dbdbdb"
        app:dashGap="2dp"
        app:dashOrientation="vertical"
        app:dashWidth="2dp"
        app:strokeW="1dp"/>
</RelativeLayout>
