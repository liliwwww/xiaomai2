<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:ratingBar="http://schemas.android.com/apk/res-auto"
    android:background="@color/color_ffffff"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <RelativeLayout
        android:id="@+id/project_item_my_evaluate_rv"
        android:background="@drawable/self_comment_entrance_bg"
        android:paddingLeft="@dimen/margin_15dp"
        android:paddingTop="@dimen/margin_24dp"
        android:paddingRight="@dimen/margin_15dp"
        android:paddingBottom="@dimen/margin_18dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/margin_21dp"
        android:layout_marginTop="@dimen/margin_18dp"
        android:layout_marginRight="@dimen/margin_21dp">
        <TextView
            android:textSize="18dp"
            android:textColor="@color/color_111111"
            android:id="@+id/project_item_my_evaluate_label_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="我的评价"
            android:layout_alignParentLeft="true"/>
        <cn.damai.uikit.iconfont.DMIconFontTextView
            android:textSize="12dp"
            android:textColor="@color/color_000000"
            android:id="@+id/project_item_my_evaluate_indicator"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_3dp"
            android:text="@string/iconfont_youjiantouxiao12"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"/>
        <TextView
            android:textSize="13dp"
            android:textColor="@color/color_999999"
            android:id="@+id/project_item_my_evaluate_time_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toLeftOf="@+id/project_item_my_evaluate_indicator"
            android:layout_centerVertical="true"/>
        <cn.damai.uikit.view.DMRatingBar
            android:id="@+id/project_item_my_evaluate_rating_level"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_9dp"
            android:layout_toRightOf="@+id/project_item_my_evaluate_label_tv"
            android:layout_centerVertical="true"
            ratingBar:isIndicator="true"
            ratingBar:showText="false"
            ratingBar:starCount="5"
            ratingBar:starDistance="1dp"
            ratingBar:starEmpty="@drawable/rating_empty"
            ratingBar:starFill="?attr/bgRatingBarStar"
            ratingBar:starSize="@dimen/margin_16dp"/>
    </RelativeLayout>
</FrameLayout>
