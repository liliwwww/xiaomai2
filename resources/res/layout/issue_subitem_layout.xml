<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:ratingbar="http://schemas.android.com/apk/res-auto"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:textSize="14dp"
        android:textColor="@color/color_111111"
        android:gravity="center"
        android:layout_gravity="center_vertical"
        android:id="@+id/issue_subitem_comment_tip"
        android:layout_width="64dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/margin_10dp"
        android:lines="1"/>
    <cn.damai.uikit.view.DMRatingBar
        android:id="@+id/issue_subitem_evaluate_grade_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        ratingbar:integerMark="true"
        ratingbar:isIndicator="false"
        ratingbar:showText="false"
        ratingbar:starCount="5"
        ratingbar:starDistance="12dp"
        ratingbar:starEmpty="@drawable/rating_empty"
        ratingbar:starFill="@drawable/rating_fill"
        ratingbar:starSize="22dp"/>
</merge>
