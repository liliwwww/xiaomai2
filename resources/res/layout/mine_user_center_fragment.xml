<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/mint_header_top_bgw"
        android:background="#f4f5f6"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ImageView
            android:id="@+id/mine_header_top_bg"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="-23dp"
            android:src="@drawable/mine_unvip_head_bg"
            android:scaleType="centerCrop"/>
    </LinearLayout>
    <cn.damai.uikit.view.FixedPtrFrameLayout
        android:id="@+id/live_ptr"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <cn.damai.uikit.view.CompatCoordinatorLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <com.google.android.material.appbar.AppBarLayout
                android:id="@+id/appbar"
                android:background="@color/transparent"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:elevation="0dp">
                <com.google.android.material.appbar.CollapsingToolbarLayout
                    android:orientation="vertical"
                    android:id="@+id/layout_toolbar"
                    android:background="@color/transparent"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="46dp"
                    app:collapsedTitleGravity="left"
                    app:collapsedTitleTextAppearance="@style/Toolbar.TitleText.Collapsed"
                    app:expandedTitleTextAppearance="@style/Toolbar.TitleText"
                    app:layout_scrollFlags="exitUntilCollapsed|scroll">
                    <RelativeLayout
                        android:background="@color/transparent"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <View
                            android:id="@+id/bottom_service_mask"
                            android:layout_width="match_parent"
                            android:layout_height="60dp"
                            android:layout_alignParentBottom="true"/>
                        <LinearLayout
                            android:orientation="vertical"
                            android:id="@+id/header_view"
                            android:background="@color/transparent"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:layout_collapseMode="parallax"
                            app:layout_collapseParallaxMultiplier="0">
                            <cn.damai.mine.view.MineHeadAllView
                                android:id="@+id/layout_mine_head_all_layout"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"/>
                            <FrameLayout
                                android:id="@+id/bg_mine_middle_container_w"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <View
                                    android:id="@+id/bg_mine_middle_container_1"
                                    android:background="@color/white"
                                    android:layout_width="match_parent"
                                    android:layout_height="60dp"/>
                                <View
                                    android:id="@+id/bg_mine_middle_container_2"
                                    android:background="#f4f5f6"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="60dp"/>
                                <LinearLayout
                                    android:orientation="vertical"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">
                                    <include layout="@layout/mine_item_notice"/>
                                    <include layout="@layout/mine_item_fixed_service"/>
                                    <include layout="@layout/mine_item_want_and_reservation"/>
                                    <include layout="@layout/mine_item_ticket_holder"/>
                                    <include layout="@layout/mine_item_dynamic_service"/>
                                    <include layout="@layout/mine_item_banner"/>
                                    <include layout="@layout/mine_logout_item"/>
                                </LinearLayout>
                            </FrameLayout>
                        </LinearLayout>
                    </RelativeLayout>
                </com.google.android.material.appbar.CollapsingToolbarLayout>
                <LinearLayout
                    android:id="@+id/dynamic_container"
                    android:background="#f4f5f6"
                    android:paddingLeft="12dp"
                    android:paddingTop="22dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textSize="18dp"
                        android:textColor="@color/color_2e333e"
                        android:gravity="center_vertical"
                        android:id="@+id/dynamic_title_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/mine_dynamic_str"/>
                    <TextView
                        android:textSize="16dp"
                        android:textColor="@color/color_2e333e"
                        android:gravity="center_vertical"
                        android:id="@+id/dynamic_num_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/margin_3dp"/>
                </LinearLayout>
                <RelativeLayout
                    android:id="@+id/pioneer_feed_tab_container"
                    android:background="#f0f3f7"
                    android:paddingLeft="18dp"
                    android:paddingTop="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true">
                    <LinearLayout
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:id="@+id/fl_pioneer_tab_title_select"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <com.alibaba.pictures.bricks.view.PuHuiTiTextView
                            android:textSize="@dimen/resource_size_18"
                            android:textColor="#181818"
                            android:layout_gravity="center"
                            android:id="@+id/pioneer_tab_title_select"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/resource_size_1"
                            android:text="@string/mine_user_center_dynamic"/>
                        <com.alibaba.pictures.bricks.view.PuHuiTiTextView
                            android:textSize="@dimen/resource_size_16"
                            android:textColor="#181818"
                            android:id="@+id/pioneer_tab_title_select_num"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="4dp"/>
                    </LinearLayout>
                </RelativeLayout>
            </com.google.android.material.appbar.AppBarLayout>
            <cn.damai.mine.userinfo.view.NoScrollViewPager
                android:id="@+id/view_pager"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:layout_behavior="@string/appbar_scrolling_view_behavior"/>
        </cn.damai.uikit.view.CompatCoordinatorLayout>
    </cn.damai.uikit.view.FixedPtrFrameLayout>
    <cn.damai.mine.view.MineUserCenterTitleNewView
        android:id="@+id/ll_mine_user_center_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <ImageView
        android:layout_gravity="end|bottom"
        android:id="@+id/submit_btn"
        android:visibility="gone"
        android:layout_width="52dp"
        android:layout_height="52dp"
        android:layout_marginBottom="@dimen/margin_12dp"
        android:src="@drawable/icon_publish_new_add"
        android:layout_marginEnd="21dp"/>
    <View
        android:layout_gravity="end|bottom"
        android:id="@+id/submit_btn_pioneer_click"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginBottom="76dp"
        android:layout_marginEnd="20dp"/>
    <ImageView
        android:layout_gravity="end|bottom"
        android:id="@+id/submit_btn_pioneer"
        android:visibility="gone"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginBottom="63dp"
        android:src="@drawable/icon_publish_new_add_pioneer"
        android:scaleType="fitXY"
        android:layout_marginEnd="6dp"/>
</FrameLayout>
