<?xml version="1.0" encoding="utf-8"?>
<cn.damai.trade.newtradeorder.ui.projectdetail.projectdetailitem.ui.view.LineLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@color/white"
    android:paddingLeft="21dp"
    android:paddingTop="12dp"
    android:paddingRight="21dp"
    android:paddingBottom="12dp"
    android:visibility="gone"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:gravity="top"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <FrameLayout
            android:id="@+id/fl_sub_tags_container"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1">
            <TextView
                android:textSize="@dimen/margin_12dp"
                android:textColor="@color/color_9C9CA5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="领券"/>
        </FrameLayout>
        <TextView
            android:textSize="@dimen/margin_12dp"
            android:textColor="@color/color_9C9CA5"
            android:layout_gravity="center_vertical"
            android:id="@+id/tv_project_coupon_mark"
            android:paddingTop="3dp"
            android:paddingRight="1dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="6dp"
            android:text="全部"
            android:includeFontPadding="false"/>
        <cn.damai.uikit.iconfont.DMIconFontTextView
            android:textSize="@dimen/margin_13dp"
            android:textColor="#3c3f44"
            android:layout_gravity="center_vertical"
            android:paddingTop="3dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/iconfont_youjiantou24"
            android:includeFontPadding="false"/>
    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/sub_marketing_item_container"
        android:paddingTop="2dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</cn.damai.trade.newtradeorder.ui.projectdetail.projectdetailitem.ui.view.LineLinearLayout>
