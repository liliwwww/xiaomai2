<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <LinearLayout
        android:orientation="vertical"
        android:background="@drawable/damai_list_bg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:textSize="16dp"
            android:textColor="@color/color_333333"
            android:id="@+id/deliver_name_text"
            android:paddingTop="14dp"
            android:paddingBottom="11dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="订单明细"/>
        <View
            android:background="#dddddd"
            android:layout_width="wrap_content"
            android:layout_height="0.5dp"
            android:layout_marginLeft="15dp"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp">
            <TextView
                android:textSize="14dp"
                android:textColor="@color/color_a9a9a9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/damai_cinemaorder_commodity_amount"
                android:layout_centerVertical="true"/>
            <TextView
                android:textSize="14dp"
                android:textColor="@color/text_color_34"
                android:id="@+id/tv_product_price"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/damai_cinemaorder_total_y"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"/>
        </RelativeLayout>
        <TextView
            android:id="@+id/rl_yunfei_line"
            android:background="@color/line_color_e5"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="9dp"
            android:layout_marginRight="9dp"/>
        <RelativeLayout
            android:id="@+id/rl_yunfei"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp">
            <TextView
                android:textSize="14dp"
                android:textColor="@color/color_a9a9a9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/damai_orderdetailnew_yes_freight_01"
                android:layout_centerVertical="true"/>
            <TextView
                android:textSize="14dp"
                android:textColor="@color/text_color_34"
                android:id="@+id/tv_yunfei_price"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/damai_cinemaorder_total_y"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"/>
        </RelativeLayout>
        <TextView
            android:id="@+id/rl_youhui_line"
            android:background="@color/line_color_e5"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="9dp"
            android:layout_marginRight="9dp"/>
        <RelativeLayout
            android:id="@+id/rl_youhui"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp">
            <TextView
                android:textSize="14dp"
                android:textColor="@color/color_a9a9a9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/damai_cinemaorder_coupons"
                android:layout_centerVertical="true"/>
            <TextView
                android:textSize="14dp"
                android:textColor="@color/text_color_34"
                android:id="@+id/tv_ticket_price"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/damai_cinemaorder_total_y"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"/>
        </RelativeLayout>
        <TextView
            android:id="@+id/rl_youhuizc_line"
            android:background="@color/line_color_e5"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="9dp"
            android:layout_marginRight="9dp"/>
        <RelativeLayout
            android:id="@+id/rl_youhuizc"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp">
            <TextView
                android:textSize="14dp"
                android:textColor="@color/color_a9a9a9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/damai_cinemaorder_01"
                android:layout_centerVertical="true"/>
            <TextView
                android:textSize="14dp"
                android:textColor="@color/text_color_34"
                android:id="@+id/tv_ticket_yhzc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/damai_cinemaorder_total_y"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"/>
        </RelativeLayout>
        <TextView
            android:id="@+id/rl_jifen_line"
            android:background="@color/line_color_e5"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="9dp"
            android:layout_marginRight="9dp"/>
        <RelativeLayout
            android:id="@+id/rl_jifen"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp">
            <TextView
                android:textSize="14dp"
                android:textColor="@color/color_a9a9a9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/order_num_01"
                android:layout_centerVertical="true"/>
            <TextView
                android:textSize="14dp"
                android:textColor="@color/text_color_34"
                android:id="@+id/tv_ticket_score"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/damai_cinemaorder_total_y"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"/>
        </RelativeLayout>
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/baoxian_price_view"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:background="@color/line_color_e5"
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_marginLeft="9dp"
                android:layout_marginRight="9dp"/>
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp">
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">
                    <TextView
                        android:textSize="14dp"
                        android:textColor="@color/text_color_34"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/order_confirm_vercode_activity_01"
                        android:layout_centerVertical="true"/>
                    <LinearLayout
                        android:gravity="center_vertical"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="4dp">
                        <TextView
                            android:textSize="11dp"
                            android:textColor="#999999"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/order_num_02"/>
                        <TextView
                            android:textSize="11dp"
                            android:textColor="#999999"
                            android:id="@+id/order_num"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="345535"/>
                    </LinearLayout>
                </LinearLayout>
                <TextView
                    android:textSize="14dp"
                    android:textColor="@color/text_color_34"
                    android:layout_gravity="center_vertical"
                    android:id="@+id/tv_baoxian_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/damai_cinemaorder_total_y"/>
            </LinearLayout>
        </LinearLayout>
        <TextView
            android:background="@color/line_color_e5"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="9dp"
            android:layout_marginRight="9dp"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp">
            <TextView
                android:textSize="14dp"
                android:textColor="@color/color_a9a9a9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/damai_cinemaorder_order_amount2"
                android:layout_centerVertical="true"/>
            <TextView
                android:textSize="14dp"
                android:textColor="@color/main_color"
                android:id="@+id/tv_order_price_danwei"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/damai_cinemaorder_total_y"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"/>
            <TextView
                android:textSize="14dp"
                android:textColor="@color/main_color"
                android:id="@+id/tv_order_price"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toLeftOf="@+id/tv_order_price_danwei"
                android:layout_centerVertical="true"/>
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>
