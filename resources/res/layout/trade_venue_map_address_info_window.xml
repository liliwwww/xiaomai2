<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@drawable/trade_venue_map_info_window_bg"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <RelativeLayout
        android:orientation="vertical"
        android:paddingLeft="@dimen/margin_12dp"
        android:paddingRight="@dimen/margin_15dp"
        android:layout_width="269dp"
        android:layout_height="wrap_content">
        <TextView
            android:textSize="@dimen/margin_13dp"
            android:textColor="@color/main_color"
            android:id="@+id/trade_venue_map_navigate_tv"
            android:background="@drawable/project_detail_perform_bg_selected"
            android:paddingLeft="@dimen/margin_16dp"
            android:paddingTop="8.5dp"
            android:paddingRight="@dimen/margin_16dp"
            android:paddingBottom="8.5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="导航"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"/>
        <TextView
            android:textSize="@dimen/margin_15dp"
            android:textColor="@color/color_222222"
            android:ellipsize="end"
            android:id="@+id/trade_venue_map_venue_name_tv"
            android:paddingTop="@dimen/margin_12dp"
            android:paddingRight="@dimen/margin_12dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:lines="1"
            android:layout_toLeftOf="@+id/trade_venue_map_navigate_tv"/>
        <TextView
            android:textSize="@dimen/margin_12dp"
            android:textColor="@color/color_999999"
            android:ellipsize="end"
            android:id="@+id/trade_venue_map_venue_detail_address_tv"
            android:paddingTop="@dimen/margin_8dp"
            android:paddingRight="@dimen/margin_12dp"
            android:paddingBottom="@dimen/margin_15dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:lines="1"
            android:layout_toLeftOf="@+id/trade_venue_map_navigate_tv"
            android:layout_below="@+id/trade_venue_map_venue_name_tv"/>
    </RelativeLayout>
</FrameLayout>
