<?xml version="1.0" encoding="utf-8"?>
<com.google.android.flexbox.FlexboxLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    app:alignItems="center"
    app:flexDirection="row"
    app:flexWrap="nowrap">
    <com.alibaba.pictures.bricks.view.PuHuiTiNumTextView
        android:textSize="@dimen/margin_15dp"
        android:textColor="@color/white"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:id="@+id/pioneer_super_card_num"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:maxLines="1"/>
    <TextView
        android:textSize="@dimen/margin_12dp"
        android:textColor="@color/white"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:id="@+id/pioneer_super_card_text"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="1.5dp"
        android:maxLines="1"/>
    <TextView
        android:textSize="@dimen/resource_size_11"
        android:textColor="@color/bricks_white"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:id="@+id/pioneer_super_card_text_sub"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:maxLines="1"
        android:layout_marginStart="@dimen/resource_size_3"/>
</com.google.android.flexbox.FlexboxLayout>
