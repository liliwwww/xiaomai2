    <cn.damai.uikit.iconfont.DMIconFontTextView
        android:textColor="#c8c8c8"
        android:id="@+id/iv_circle_check"
        android:layout_width="18dp"
        android:layout_height="18dp"
        android:text="@string/iconfont_yuangouxuanweixuanzhong"/>
</LinearLayout>
