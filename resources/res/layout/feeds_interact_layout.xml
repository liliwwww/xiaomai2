<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:gravity="center"
    android:orientation="horizontal"
    android:id="@+id/feeds_interact_parent"
    android:layout_width="match_parent"
    android:layout_height="20dp"
    android:layout_marginRight="18dp">
    <TextView
        android:textSize="12dp"
        android:textColor="#9c9ca5"
        android:gravity="center"
        android:id="@+id/user_feeds_zhuanfatv"
        android:focusable="true"
        android:visibility="gone"
        android:clickable="true"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_marginRight="3dp"
        android:text="转载"/>
    <cn.damai.uikit.iconfont.DMIconFontTextView
        android:textSize="14dp"
        android:textColor="#3c3f44"
        android:gravity="center"
        android:id="@+id/user_feeds_zhuanfaicon"
        android:focusable="true"
        android:visibility="gone"
        android:clickable="true"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:layout_marginRight="18dp"
        android:text="@string/iconfont_zhuanzai_"/>
    <TextView
        android:textSize="12dp"
        android:textColor="#9c9ca5"
        android:gravity="center"
        android:id="@+id/user_feeds_commenttv"
        android:focusable="true"
        android:clickable="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="3dp"
        android:text=""/>
    <cn.damai.uikit.iconfont.DMIconFontTextView
        android:textSize="14dp"
        android:textColor="#3c3f44"
        android:gravity="center"
        android:id="@+id/user_feeds_commenticon"
        android:focusable="true"
        android:clickable="true"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:layout_marginRight="18dp"
        android:text="@string/iconfont_pinglun_"/>
    <TextView
        android:textSize="12dp"
        android:textColor="#3c3f44"
        android:gravity="center"
        android:id="@+id/user_feeds_zantv"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_marginRight="3dp"
        android:text=""/>
    <cn.damai.uikit.iconfont.DMIconFontTextView
        android:textSize="14dp"
        android:textColor="#3c3f44"
        android:gravity="center"
        android:id="@+id/user_feeds_zanicon"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:layout_marginRight="18dp"
        android:text="@string/iconfont_dianzan_"/>
    <cn.damai.uikit.iconfont.DMIconFontTextView
        android:textSize="14dp"
        android:textColor="#3c3f44"
        android:id="@+id/feeds_item_more"
        android:padding="5dp"
        android:focusable="true"
        android:visibility="gone"
        android:clickable="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/iconfont_gengduo16"
        android:layout_alignParentRight="true"/>
</LinearLayout>
