<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingTop="@dimen/resource_size_9"
    android:clipChildren="false"
    android:clipToPadding="false"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingStart="@dimen/resource_size_12"
    android:paddingEnd="@dimen/resource_size_12">
    <androidx.cardview.widget.CardView
        android:id="@+id/card_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="12dp"
        app:cardElevation="0dp"
        app:cardMaxElevation="0dp">
        <RelativeLayout
            android:id="@+id/super_card_project_info_container"
            android:layout_width="match_parent"
            android:layout_height="150dp">
            <com.alibaba.pictures.moimage.MoImageView
                android:id="@+id/pioneer_super_card_bg"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="centerCrop"
                app:failureImage="@drawable/pioneer_default_image_bg_gradient"
                app:placeholderImage="@drawable/pioneer_default_image_bg_gradient"/>
            <View
                android:id="@+id/pioneer_super_card_grey"
                android:background="#80000000"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>
            <View
                android:id="@+id/pioneer_super_card_shadow"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>
            <androidx.cardview.widget.CardView
                android:id="@+id/pioneer_super_cd_card_poster"
                android:layout_width="112dp"
                android:layout_height="match_parent"
                android:backgroundTint="@color/transparent"
                app:cardCornerRadius="12dp"
                app:cardElevation="0dp"
                app:cardUseCompatPadding="false">
                <com.alibaba.pictures.moimage.MoImageView
                    android:id="@+id/pioneer_super_card_poster"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="centerCrop"
                    app:failureImage="@drawable/home_default_image_bg_gradient_pioneer"
                    app:failureImageScaleType="fitXY"
                    app:placeholderImage="@drawable/home_default_image_bg_gradient_pioneer"
                    app:placeholderImageScaleType="fitXY"/>
            </androidx.cardview.widget.CardView>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="9dp"
                android:layout_marginStart="5dp"
                android:layout_alignEnd="@+id/pioneer_super_cd_card_poster">
                <com.alibaba.pictures.bricks.view.PuHuiTiTextView
                    android:textSize="@dimen/margin_10dp"
                    android:textColor="@color/white"
                    android:gravity="center"
                    android:id="@+id/pioneer_super_card_movie_flag"
                    android:background="@drawable/bg_border_corner_e6000000_6"
                    android:paddingLeft="@dimen/margin_4dp"
                    android:paddingRight="@dimen/margin_4dp"
                    android:paddingBottom="0.5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="18dp"
                    android:includeFontPadding="false"/>
                <androidx.cardview.widget.CardView
                    android:layout_width="@dimen/resource_size_43"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="2dp"
                    app:cardCornerRadius="6dp"
                    app:cardElevation="0dp"
                    app:cardUseCompatPadding="false">
                    <LinearLayout
                        android:gravity="center_vertical"
                        android:id="@+id/pioneer_ll_super_card_hasreserved"
                        android:background="@drawable/bg_pioneer_has_reserved"
                        android:paddingLeft="@dimen/resource_size_2"
                        android:paddingRight="@dimen/resource_size_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:layout_width="@dimen/resource_size_9"
                            android:layout_height="@dimen/resource_size_9"
                            android:src="@drawable/pioneer_super_card_hasreserved_icon"/>
                        <com.alibaba.pictures.bricks.view.PuHuiTiTextView
                            android:textSize="@dimen/resource_size_9"
                            android:textColor="@color/bricks_171717"
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/pioneer_super_card_hasreserved"
                            android:paddingBottom="0.5dp"
                            android:layout_width="wrap_content"
                            android:layout_height="18dp"
                            android:text="已预约"
                            android:maxLines="1"
                            android:includeFontPadding="false"
                            android:layout_marginStart="@dimen/resource_size_1"/>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/pioneer_super_ll_cd_card_info"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/resource_size_4"
                android:layout_toRightOf="@+id/pioneer_super_cd_card_poster"
                android:layout_marginStart="3dp"
                android:layout_marginEnd="20dp">
                <com.alibaba.pictures.bricks.view.FakeBoldTextView
                    android:textSize="@dimen/resource_size_16"
                    android:textColor="@color/white"
                    android:ellipsize="end"
                    android:id="@+id/pioneer_super_card_title"
                    android:paddingLeft="@dimen/margin_6dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:maxLines="2"
                    android:lineSpacingExtra="1px"/>
                <TextView
                    android:textSize="@dimen/resource_size_11"
                    android:textColor="#b3ffffff"
                    android:ellipsize="end"
                    android:id="@+id/pioneer_super_card_actorname_brand"
                    android:paddingLeft="@dimen/margin_6dp"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/resource_size_3"
                    android:maxLines="1"/>
                <LinearLayout
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:id="@+id/pioneer_super_card_ll_want_score"
                    android:paddingLeft="@dimen/margin_6dp"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/margin_22dp"
                    android:layout_marginTop="0.5dp">
                    <cn.damai.pioneer.component.supercard.SuperCardNumTextView
                        android:id="@+id/pioneer_super_card_want_score"
                        android:visibility="gone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                    <View
                        android:id="@+id/pioneer_super_card_line"
                        android:background="@drawable/bg_line_corner_66ffffff_1"
                        android:visibility="gone"
                        android:layout_width="@dimen/margin_1dp"
                        android:layout_height="@dimen/margin_11dp"
                        android:layout_marginLeft="@dimen/margin_6dp"
                        android:layout_marginRight="@dimen/margin_6dp"/>
                    <cn.damai.pioneer.component.supercard.SuperCardNumTextView
                        android:id="@+id/pioneer_super_card_subtitle"
                        android:visibility="gone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"/>
                </LinearLayout>
                <com.alibaba.pictures.bricks.view.DMDigitDinBoldTextView
                    android:textSize="@dimen/resource_size_11"
                    android:textColor="#b3ffffff"
                    android:ellipsize="end"
                    android:id="@+id/pioneer_super_card_time"
                    android:paddingLeft="@dimen/margin_6dp"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="1.5dp"
                    android:maxLines="1"/>
                <TextView
                    android:textSize="@dimen/resource_size_11"
                    android:textColor="#b3ffffff"
                    android:ellipsize="end"
                    android:id="@+id/pioneer_super_card_venue"
                    android:paddingLeft="@dimen/margin_6dp"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3.5dp"
                    android:maxLines="1"/>
                <LinearLayout
                    android:gravity="bottom"
                    android:paddingLeft="@dimen/margin_5dp"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginBottom="@dimen/resource_size_10"
                    android:layout_weight="1">
                    <cn.damai.pioneer.component.supercard.SuperCardCountDownView
                        android:orientation="horizontal"
                        android:id="@+id/pioneer_super_card_count_down"
                        android:paddingLeft="@dimen/margin_1dp"
                        android:visibility="gone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/margin_4dp"/>
                    <com.alibaba.pictures.bricks.view.PuHuiTiTextView
                        android:textSize="@dimen/resource_size_15"
                        android:textColor="@color/bricks_white"
                        android:id="@+id/live_pioneer_low_price_unit"
                        android:visibility="gone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="0.5dp"
                        android:layout_marginTop="-1.5dp"
                        android:includeFontPadding="false"
                        android:layout_marginEnd="-0.5dp"/>
                    <cn.damai.pioneer.component.supercard.SuperCardNumTextView
                        android:id="@+id/pioneer_super_card_num_desc_view"
                        android:visibility="gone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:includeFontPadding="false"/>
                </LinearLayout>
            </LinearLayout>
        </RelativeLayout>
    </androidx.cardview.widget.CardView>
    <com.alibaba.pictures.moimage.MoImageView
        android:layout_gravity="end"
        android:id="@+id/pioneer_super_card_right_tag"
        android:layout_width="77dp"
        android:layout_height="77dp"
        android:layout_marginTop="-12dp"
        android:layout_marginEnd="-12dp"
        android:layout_alignParentEnd="true"
        android:translationZ="10dp"/>
</RelativeLayout>
