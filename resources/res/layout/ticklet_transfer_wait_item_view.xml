<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@color/white"
    android:paddingBottom="28dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <include layout="@layout/member_divider_layout"/>
    <TextView
        android:textSize="@dimen/margin_18dp"
        android:textColor="#111111"
        android:gravity="center_horizontal"
        android:id="@+id/ticklet_detail_accept_num"
        android:paddingTop="28dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="24dp"/>
    <TextView
        android:gravity="center_horizontal"
        android:id="@+id/ticklet_detail_accept_invalid_time"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="@dimen/margin_9dp"
        android:layout_marginRight="24dp"
        style="@style/ticklet_ff2d79_13dp"/>
    <View
        android:background="@drawable/ticklet_transfer_wait_point_bg"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_marginTop="@dimen/margin_20dp"
        android:layout_marginBottom="@dimen/margin_20dp"/>
    <TextView
        android:textSize="@dimen/margin_16dp"
        android:textColor="#111111"
        android:id="@+id/ticklet_detail_accept_perform_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="44dp"
        android:layout_marginRight="44dp"
        android:layout_marginBottom="@dimen/margin_6dp"
        android:lineSpacingExtra="@dimen/margin_2dp"/>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="44dp"
        android:layout_marginTop="@dimen/margin_12dp"
        android:layout_marginRight="44dp">
        <cn.damai.uikit.iconfont.DMIconFontTextView
            android:visibility="visible"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="4dp"
            android:text="@string/iconfont_shijian16"
            style="@style/ticklet_000000_16dp"/>
        <TextView
            android:id="@+id/ticklet_transfer_tv_show_time_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/ticklet_000000_14dp"/>
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:id="@+id/ticklet_transfer_wait_address_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="44dp"
        android:layout_marginTop="@dimen/margin_12dp"
        android:layout_marginRight="44dp">
        <cn.damai.uikit.iconfont.DMIconFontTextView
            android:visibility="visible"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="4dp"
            android:text="@string/iconfont_dizhi12"
            style="@style/ticklet_000000_14dp"/>
        <TextView
            android:id="@+id/ticklet_transfer_tv_address_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/ticklet_000000_14dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_gravity="center_horizontal"
        android:orientation="horizontal"
        android:id="@+id/ticklet_accept_transfer_ll_seat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="44dp"
        android:layout_marginTop="@dimen/margin_12dp"
        android:layout_marginRight="44dp">
        <cn.damai.uikit.iconfont.DMIconFontTextView
            android:visibility="visible"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:layout_marginRight="4dp"
            android:text="@string/iconfont_zuowei16"
            style="@style/ticklet_000000_14dp"/>
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/ticklet_accept_transfer_ll_seat_content"
            android:background="@color/color_ffffff"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="@style/ticklet_000000_14dp"/>
        </LinearLayout>
    </LinearLayout>
    <cn.damai.uikit.view.DMActionButtonBgView
        android:gravity="center"
        android:id="@+id/ticklet_accept_transfer_confirm"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginLeft="38dp"
        android:layout_marginTop="@dimen/margin_21dp"
        android:layout_marginRight="38dp"
        android:layout_marginBottom="@dimen/margin_8dp"
        android:layout_below="@+id/indicator_container"/>
</LinearLayout>
