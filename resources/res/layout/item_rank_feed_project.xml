<?xml version="1.0" encoding="utf-8"?>
<cn.damai.uikit.view.StrokeLinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@drawable/bg_rank_item_project"
    android:paddingBottom="@dimen/margin_12dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/margin_9dp"
    app:sl_corners_radius="8dp">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:background="@drawable/bg_circle_card"
        android:paddingTop="@dimen/margin_13dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingStart="@dimen/margin_12dp"
        android:paddingEnd="@dimen/margin_12dp">
        <cn.damai.uikit.view.DMPosterView
            android:id="@+id/poster"
            android:layout_width="72dp"
            android:layout_height="102dp"
            android:layout_marginTop="2dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <cn.damai.uikit.number.DMDigitTextView
            android:textSize="20dp"
            android:textColor="@color/white"
            android:gravity="center"
            android:id="@+id/tv_order"
            android:background="@drawable/bg_rank_project_item_order1"
            android:paddingBottom="3dp"
            android:layout_width="@dimen/margin_23dp"
            android:layout_height="@dimen/margin_30dp"
            android:layout_marginTop="2dp"
            android:layout_marginStart="6dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <TextView
            android:textSize="16dp"
            android:textColor="#2e333e"
            android:ellipsize="end"
            android:id="@+id/tv_title"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:maxLines="2"
            android:lineSpacingExtra="2dp"
            android:paddingStart="12dp"
            android:paddingEnd="12dp"
            app:layout_constraintEnd_toStartOf="@+id/tv_buy"
            app:layout_constraintStart_toEndOf="@+id/poster"
            app:layout_constraintTop_toTopOf="parent"/>
        <cn.damai.uikit.view.SeeAnimateView
            android:id="@+id/wanna_2_see_icon"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginTop="2dp"
            app:layout_constraintEnd_toEndOf="@+id/tv_buy"
            app:layout_constraintStart_toEndOf="@+id/linearLayout4"
            app:layout_constraintTop_toTopOf="parent"/>
        <ImageView
            android:id="@+id/block_wanna_2_see_icon"
            android:visibility="gone"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginTop="2dp"
            android:src="@drawable/icon_block_want_gray"
            android:scaleType="centerInside"
            app:layout_constraintEnd_toEndOf="@+id/tv_buy"
            app:layout_constraintStart_toEndOf="@+id/linearLayout4"
            app:layout_constraintTop_toTopOf="parent"/>
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/linearLayout4"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/margin_12dp"
            app:layout_constraintEnd_toEndOf="@+id/tv_title"
            app:layout_constraintStart_toStartOf="@+id/tv_title"
            app:layout_constraintTop_toBottomOf="@+id/tv_title">
            <TextView
                android:textSize="12dp"
                android:textColor="#ff901c"
                android:id="@+id/tv_desc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_2dp"/>
            <cn.damai.uikit.number.DMDigitTextView
                android:textSize="14dp"
                android:textColor="#ff901c"
                android:id="@+id/tv_desc_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:layout_marginStart="1dp"/>
            <View
                android:layout_gravity="bottom"
                android:id="@+id/view"
                android:background="#e9ebef"
                android:layout_width="1dp"
                android:layout_height="@dimen/margin_12dp"
                android:layout_marginBottom="3dp"
                android:layout_marginStart="6dp"
                android:layout_marginEnd="6dp"/>
            <cn.damai.uikit.number.DMDigitTextView
                android:textSize="14dp"
                android:textColor="#4d4d56"
                android:id="@+id/tv_price_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:paddingEnd="1dp"/>
            <TextView
                android:textSize="12dp"
                android:textColor="#4d4d56"
                android:id="@+id/tv_price"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_2dp"
                android:text="元起"/>
            <TextView
                android:textSize="12dp"
                android:textColor="#4d4d56"
                android:ellipsize="end"
                android:id="@+id/tv_price_custom"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_2dp"
                android:maxWidth="100dp"
                android:text="自定义价格自定义价格"
                android:maxLines="1"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linearLayout5"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:layout_marginStart="@dimen/margin_12dp"
            app:layout_constraintEnd_toEndOf="@+id/tv_title"
            app:layout_constraintStart_toStartOf="@+id/tv_title"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout4">
            <TextView
                android:textSize="@dimen/margin_12dp"
                android:textColor="#4d4d56"
                android:ellipsize="end"
                android:id="@+id/tv_venue"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:maxLines="1"/>
            <cn.damai.uikit.number.DMDigitTextView
                android:textSize="13dp"
                android:textColor="#4d4d56"
                android:ellipsize="end"
                android:id="@+id/tv_venue_dis"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="1dp"
                android:maxLines="1"
                android:layout_marginStart="1dp"/>
        </LinearLayout>
        <TextView
            android:textSize="12dp"
            android:textColor="#4d4d56"
            android:id="@+id/tv_time"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:layout_marginStart="@dimen/margin_12dp"
            app:layout_constraintEnd_toEndOf="@+id/tv_title"
            app:layout_constraintStart_toStartOf="@+id/tv_title"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout5"/>
        <com.alibaba.pictures.bricks.view.PuHuiTiTextView
            android:textColor="#ff79bd"
            android:gravity="center"
            android:id="@+id/tv_buy"
            android:background="@drawable/bg_rank_item_project_buy"
            android:layout_width="54dp"
            android:layout_height="27dp"
            android:text="购票"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <LinearLayout
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:id="@+id/id_h_project_rec_layout"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/margin_12dp"
        android:layout_marginTop="6dp"
        android:layout_marginRight="@dimen/margin_12dp">
        <TextView
            android:textSize="12dp"
            android:textColor="@color/color_4D4D56"
            android:id="@+id/id_h_project_rec_prefix_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <View
            android:id="@+id/id_h_project_rec_div"
            android:background="#e9ebef"
            android:layout_width="1dp"
            android:layout_height="@dimen/margin_12dp"
            android:layout_marginStart="6dp"
            android:layout_marginEnd="6dp"/>
        <TextView
            android:textSize="12dp"
            android:textColor="@color/color_4D4D56"
            android:ellipsize="end"
            android:id="@+id/id_h_project_rec_postfix_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:lines="1"/>
        <cn.damai.uikit.tag.DMCommonTagView
            android:id="@+id/id_h_project_promotion_tag"
            android:visibility="visible"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="3dp"/>
    </LinearLayout>
</cn.damai.uikit.view.StrokeLinearLayout>
