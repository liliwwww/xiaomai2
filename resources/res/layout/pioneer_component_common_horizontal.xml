<?xml version="1.0" encoding="utf-8"?>
<cn.damai.uikit.view.round.RoundRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:rRadius="@dimen/resource_size_9">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/card_container"
        android:background="@color/bricks_white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <cn.damai.uikit.view.round.RoundRelativeLayout
            android:id="@+id/roundRelativeLayout"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent="0.31"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0"
            app:rRadius="@dimen/resource_size_9">
            <com.alibaba.pictures.moimage.MoImageView
                android:id="@+id/iv_bg"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/home_default_image_bg_pioneer"
                app:failureImage="@drawable/home_default_image_bg_pioneer"
                app:placeholderImage="@drawable/home_default_image_bg_pioneer"/>
        </cn.damai.uikit.view.round.RoundRelativeLayout>
        <com.alibaba.pictures.bricks.view.PuHuiTiTextView
            android:textSize="14dp"
            android:textColor="@color/color_181818"
            android:id="@+id/tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="2dp"
            android:layout_marginStart="12dp"
            app:layout_constraintBottom_toTopOf="@+id/horizontal_container"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <com.alibaba.pictures.moimage.MoImageView
            android:id="@+id/iv_special_bg"
            android:paddingTop="3dp"
            android:layout_width="45dp"
            android:layout_height="0dp"
            android:src="@drawable/pioneer_bg_shape_calendar"
            app:layout_constraintBottom_toTopOf="@+id/horizontal_container"
            app:layout_constraintStart_toEndOf="@+id/tv_title"
            app:layout_constraintTop_toTopOf="@+id/tv_title"
            app:layout_constraintVertical_bias="1"/>
        <com.alibaba.pictures.moimage.MoImageView
            android:id="@+id/iv_more"
            android:background="@drawable/bg_border_corner4_all_000000"
            android:padding="2dp"
            android:layout_width="14dp"
            android:layout_height="14dp"
            android:layout_marginTop="2dp"
            android:src="@drawable/pioneer_card_all_arrow"
            android:layout_marginEnd="12dp"
            app:layout_constraintBottom_toTopOf="@+id/horizontal_container"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <com.alibaba.pictures.bricks.view.ProgrammaticallyScrollableRecyclerView
            android:id="@+id/horizontal_container"
            android:background="@drawable/bg_border_corner6_all_fffff"
            android:paddingTop="4dp"
            android:paddingBottom="3dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="32dp"
            android:overScrollMode="never"
            android:paddingStart="10dp"
            android:paddingEnd="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1"/>
        <com.alibaba.pictures.moimage.MoImageView
            android:id="@+id/iv_calender_horn"
            android:layout_width="20dp"
            android:layout_height="25dp"
            android:src="@drawable/pioneer_calendar_horn"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
        <com.alibaba.pictures.moimage.MoImageView
            android:id="@+id/iv_calender_point"
            android:layout_width="3dp"
            android:layout_height="6dp"
            android:layout_marginBottom="-3dp"
            android:src="@drawable/pioneer_point_calendar"
            android:layout_marginStart="13dp"
            app:layout_constraintBottom_toTopOf="@+id/horizontal_container"
            app:layout_constraintStart_toStartOf="parent"/>
        <com.alibaba.pictures.moimage.MoImageView
            android:id="@+id/iv_calender_point_1"
            android:layout_width="3dp"
            android:layout_height="6dp"
            android:layout_marginBottom="-3dp"
            android:src="@drawable/pioneer_point_calendar"
            android:layout_marginStart="148dp"
            app:layout_constraintBottom_toTopOf="@+id/horizontal_container"
            app:layout_constraintStart_toStartOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</cn.damai.uikit.view.round.RoundRelativeLayout>
