<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="@dimen/margin_18dp"
    android:layout_marginRight="@dimen/margin_18dp"
    android:layout_marginBottom="@dimen/margin_20dp">
    <TextView
        android:textSize="@dimen/margin_12dp"
        android:textColor="@color/color_111111"
        android:id="@+id/ticklet_transfer_ticket_item_seat_info_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        android:textSize="@dimen/margin_12dp"
        android:textColor="@color/color_111111"
        android:id="@+id/ticklet_transfer_ticket_item_cert_num_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_3dp"/>
</LinearLayout>
