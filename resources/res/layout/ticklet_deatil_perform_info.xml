<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ImageView
        android:id="@+id/tv_user_image"
        android:background="@drawable/ticket_card_user_info_bg"
        android:layout_width="200dp"
        android:layout_height="117dp"
        android:scaleType="fitXY"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"/>
    <ImageView
        android:id="@+id/tickelt_card_normal_diamond_icon"
        android:visibility="gone"
        android:layout_width="109dp"
        android:layout_height="110dp"
        android:layout_marginTop="3dp"
        android:src="@drawable/ticket_card_pink_vip_lg"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="-10dp"/>
    <androidx.cardview.widget.CardView
        android:id="@+id/img_item_card"
        android:layout_width="80dp"
        android:layout_height="106dp"
        android:layout_marginTop="@dimen/margin_12dp"
        android:layout_marginBottom="@dimen/margin_12dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="@dimen/margin_12dp"
        android:layout_marginEnd="@dimen/margin_8dp"
        android:layout_alignParentStart="true"
        app:cardBackgroundColor="#1a000000"
        app:cardCornerRadius="8dp"
        app:cardElevation="0dp">
        <com.taobao.uikit.extend.feature.view.TUrlImageView
            android:layout_gravity="center"
            android:id="@+id/tv_project_image"
            android:layout_width="79dp"
            android:layout_height="105dp"
            android:scaleType="centerCrop"
            app:uik_error_background="@drawable/ticklet_project_default_image"
            app:uik_place_hold_foreground="@drawable/ticklet_project_default_image"/>
    </androidx.cardview.widget.CardView>
    <LinearLayout
        android:gravity="center_vertical"
        android:orientation="vertical"
        android:id="@+id/ticklet_perform_right_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="12dp"
        android:minHeight="106dp"
        android:layout_toRightOf="@+id/img_item_card"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_centerVertical="true"
        android:layout_marginEnd="@dimen/margin_12dp">
        <TextView
            android:textSize="16dp"
            android:textColor="#2e333e"
            android:ellipsize="end"
            android:id="@+id/tv_perform_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxLines="3"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"/>
        <TextView
            android:textSize="@dimen/dis_13dp"
            android:textColor="#2e333e"
            android:ellipsize="end"
            android:gravity="center_vertical"
            android:id="@+id/tv_perform_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_6dp"
            android:maxLines="3"
            android:lineSpacingExtra="@dimen/margin_2dp"/>
        <TextView
            android:textSize="12dp"
            android:textColor="#2e333e"
            android:ellipsize="end"
            android:id="@+id/tv_perform_venue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_5dp"
            android:maxLines="1"
            style="@style/ticklet_white_14dp"/>
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/ticklet_ll_perform_pass_ticket"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_6dp">
            <TextView
                android:textSize="12dp"
                android:textColor="#959aa5"
                android:id="@+id/tv_perform_pass_ticket_tip"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="通票使用说明"/>
            <cn.damai.uikit.iconfont.DMIconFontTextView
                android:textSize="12dp"
                android:textColor="#959aa5"
                android:layout_gravity="center_vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin_1dp"
                android:layout_marginTop="@dimen/margin_1dp"
                android:text="@string/iconfont_youjiantou24"/>
        </LinearLayout>
    </LinearLayout>
</merge>
