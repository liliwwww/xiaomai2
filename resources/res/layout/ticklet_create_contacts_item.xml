<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:paddingBottom="26dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/ticklet_contacts_name"
        android:layout_width="83dp"
        android:layout_height="wrap_content"
        style="@style/ticklet_000000_13dp"/>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1">
        <TextView
            android:id="@+id/ticklet_contacts_id_card_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="3dp"
            style="@style/ticklet_666666_13dp"/>
        <TextView
            android:id="@+id/ticklet_contacts_id_number"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_6dp"
            style="@style/ticklet_666666_13dp"/>
    </LinearLayout>
    <cn.damai.uikit.iconfont.DMIconFontTextView
        android:textSize="@dimen/margin_16dp"
        android:textColor="@color/color_999999"
        android:layout_gravity="center_vertical"
        android:id="@+id/ticklet_info_choose_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/margin_6dp"
        android:text="@string/iconfont_weigouxuan"/>
</LinearLayout>
