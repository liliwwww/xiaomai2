<?xml version="1.0" encoding="utf-8"?>
<cn.damai.uikit.view.StrokeLinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/note_stroke_layout"
    android:background="@color/white"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:sl_corners_radius="6dp"
    app:sl_stroke_color="#0d000000"
    app:sl_stroke_width="1px">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <cn.damai.tetris.component.girl.mvp.HWRatioLayout
            android:id="@+id/note_s1_hw_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <cn.damai.commonbusiness.lazyimg.view.GifCareImageView
                android:id="@+id/note_s1_img"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="centerCrop"/>
        </cn.damai.tetris.component.girl.mvp.HWRatioLayout>
        <FrameLayout
            android:id="@+id/note_s2_ui"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textSize="14dp"
                android:textColor="@color/white"
                android:ellipsize="end"
                android:id="@+id/note_s2_text"
                android:paddingLeft="24dp"
                android:paddingTop="40dp"
                android:paddingRight="24dp"
                android:paddingBottom="30dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:maxLines="5"
                android:minLines="2"
                android:lineSpacingMultiplier="1.5"/>
            <ImageView
                android:layout_width="26dp"
                android:layout_height="19dp"
                android:layout_marginLeft="12dp"
                android:layout_marginTop="20dp"
                android:src="@drawable/icon_discover_yinhao"/>
        </FrameLayout>
        <com.alibaba.pictures.bricks.view.FakeBoldTextView
            android:textSize="@dimen/resource_size_11"
            android:textColor="@color/white"
            android:gravity="center"
            android:layout_gravity="bottom|right"
            android:id="@+id/note_si_top"
            android:background="@drawable/bg_note_card_top_pioneer"
            android:visibility="gone"
            android:layout_width="34dp"
            android:layout_height="18dp"
            android:layout_marginBottom="@dimen/resource_size_4"
            android:text="@string/note_si_top"
            android:layout_marginEnd="9dp"/>
        <ImageView
            android:layout_gravity="top|left"
            android:id="@+id/note_si_feature"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/bricks_feed_note_card_feature"
            android:layout_marginStart="9dp"/>
        <LinearLayout
            android:layout_gravity="top|right"
            android:orientation="horizontal"
            android:id="@+id/video_icon"
            android:paddingLeft="3dp"
            android:paddingRight="3dp"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="18dp"
            android:layout_marginTop="@dimen/resource_size_9"
            android:layout_marginRight="6dp"
            android:layout_marginBottom="6dp">
            <ImageView
                android:layout_gravity="center_vertical"
                android:visibility="visible"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_note_card_video"
                android:scaleType="fitXY"/>
            <TextView
                android:textSize="10dp"
                android:textColor="@color/white"
                android:layout_gravity="center_vertical"
                android:id="@+id/icon_play_mini_tv"
                android:visibility="gone"
                android:layout_width="0dp"
                android:layout_height="0dp"/>
        </LinearLayout>
        <ImageView
            android:layout_gravity="bottom|right"
            android:id="@+id/video_icon_old"
            android:visibility="gone"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:layout_marginRight="6dp"
            android:layout_marginBottom="6dp"
            android:src="@drawable/feed_video_icon"
            android:scaleType="fitXY"/>
    </FrameLayout>
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/bottom_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:textSize="13dp"
            android:textColor="#171717"
            android:ellipsize="end"
            android:id="@+id/note_s1_title"
            android:paddingLeft="9dp"
            android:paddingRight="9dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="9dp"
            android:maxLines="2"/>
        <LinearLayout
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingLeft="9dp"
            android:paddingRight="9dp"
            android:layout_width="match_parent"
            android:layout_height="16.5dp"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="9dp">
            <RelativeLayout
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_marginTop="1dp">
                <cn.damai.uikit.view.RoundImageView
                    android:id="@+id/user_head"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:layout_centerVertical="true"
                    app:type="linear"/>
                <ImageView
                    android:id="@+id/user_head_tag"
                    android:layout_width="8dp"
                    android:layout_height="8dp"
                    android:scaleType="fitXY"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentBottom="true"/>
            </RelativeLayout>
            <TextView
                android:textSize="12dp"
                android:textColor="#959aa5"
                android:ellipsize="end"
                android:gravity="center_vertical"
                android:id="@+id/user_name"
                android:paddingLeft="3dp"
                android:paddingRight="3dp"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:lines="1"
                android:layout_weight="1"/>
            <LinearLayout
                android:gravity="center_vertical"
                android:id="@+id/watch_count_ui"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <cn.damai.uikit.iconfont.DMIconFontTextView
                    android:textSize="12dp"
                    android:textColor="#9c9ca5"
                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                    android:id="@+id/watch_count_icon"
                    android:paddingTop="1dp"
                    android:layout_width="wrap_content"
                    android:layout_height="15dp"
                    android:text="@string/iconfont_faxian28"/>
                <TextView
                    android:textSize="10dp"
                    android:textColor="@color/color_9C9CA5"
                    android:ellipsize="end"
                    android:id="@+id/watch_count"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="3dp"
                    android:lines="1"/>
            </LinearLayout>
            <LinearLayout
                android:gravity="center_vertical"
                android:id="@+id/ll_dna"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginBottom="0.5dp">
                <ImageView
                    android:id="@+id/iv_dna_pic"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="0.5dp"
                    android:src="@drawable/icon_discover_content_detail_dna_mini"/>
                <cn.damai.uikit.number.DMDigitTextView
                    android:textSize="11dp"
                    android:textColor="@color/color_dna"
                    android:ellipsize="end"
                    android:id="@+id/tv_dna_similarity"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:lines="1"/>
            </LinearLayout>
            <TextView
                android:textSize="10dp"
                android:textColor="@color/color_9C9CA5"
                android:ellipsize="end"
                android:id="@+id/new_common_text"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="3dp"
                android:lines="1"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/ll_card_note_proj"
            android:background="@drawable/bg_common_gray_radius4"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_marginLeft="6dp"
            android:layout_marginRight="6dp"
            android:layout_marginBottom="9dp">
            <RelativeLayout
                android:layout_width="27dp"
                android:layout_height="36dp"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="3dp">
                <cn.damai.uikit.view.round.RoundImageView2
                    android:id="@+id/ll_card_note_proj_img"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerInParent="true"
                    app:rRadius="2dp"/>
                <cn.damai.uikit.number.DMDigitTextView
                    android:textSize="10dp"
                    android:textColor="@color/white"
                    android:id="@+id/ll_card_note_proj_score"
                    android:background="@drawable/card_note_project_scorebg"
                    android:padding="1.5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </RelativeLayout>
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="4dp"
                android:layout_marginRight="4dp">
                <TextView
                    android:textSize="10dp"
                    android:textColor="@color/color_333333"
                    android:ellipsize="end"
                    android:id="@+id/ll_card_note_proj_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:maxLines="1"/>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:layout_marginRight="12dp">
                    <TextView
                        android:textSize="10dp"
                        android:textColor="@color/color_FF2869"
                        android:id="@+id/dfp_price_prefix"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="1dp"
                        android:text="¥"/>
                    <cn.damai.uikit.number.DMDigitTextView
                        android:textSize="13dp"
                        android:textColor="#ff2869"
                        android:id="@+id/dfp_price"
                        android:paddingTop="2dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="0.5dp"
                        android:layout_marginTop="1dp"
                        android:singleLine="true"/>
                    <TextView
                        android:textSize="10dp"
                        android:textColor="@color/color_666666"
                        android:id="@+id/dfp_price_postfix"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="1dp"
                        android:layout_marginBottom="1dp"
                        android:text="起"
                        android:includeFontPadding="false"/>
                    <cn.damai.uikit.number.DMDigitTextView
                        android:textSize="12dp"
                        android:textColor="#ff2869"
                        android:id="@+id/dfp_price_pending"
                        android:visibility="gone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="价格待定"
                        android:singleLine="true"/>
                    <FrameLayout
                        android:layout_gravity="center_vertical"
                        android:id="@+id/market_tag_container"
                        android:visibility="gone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="2dp"
                        android:layout_marginTop="2dp"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/note_card_bottom_ll"
            android:background="@drawable/card_note_project_descbg"
            android:paddingLeft="3.5dp"
            android:paddingTop="2.5dp"
            android:paddingRight="6dp"
            android:paddingBottom="2.5dp"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="6dp"
            android:layout_marginBottom="9dp">
            <ImageView
                android:layout_gravity="center_vertical"
                android:id="@+id/note_card_bottom_icon"
                android:paddingTop="1dp"
                android:layout_width="11dp"
                android:layout_height="11dp"
                android:src="@drawable/home_card_note_tipicon"/>
            <TextView
                android:textSize="11dp"
                android:textColor="#ff903c"
                android:id="@+id/note_card_bottom_desc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2dp"/>
        </LinearLayout>
    </LinearLayout>
</cn.damai.uikit.view.StrokeLinearLayout>
