<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:gravity="center_vertical"
    android:orientation="horizontal"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <TextView
        android:textSize="10dp"
        android:textColor="@color/white"
        android:gravity="center_vertical"
        android:id="@+id/project_prefix_tag_view"
        android:background="@drawable/bg_border_corner_ffebef_stroke_3"
        android:layout_width="wrap_content"
        android:layout_height="15dp"
        android:paddingStart="3dp"
        android:paddingEnd="3dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <TextView
        android:textSize="10dp"
        android:textColor="@color/white"
        android:gravity="center_vertical"
        android:id="@+id/project_tag_view"
        android:background="@drawable/bg_border_corner_ffebef_stroke_3"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="15dp"
        android:paddingStart="3dp"
        android:paddingEnd="3dp"
        android:layout_marginStart="-3px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/project_prefix_tag_view"
        app:layout_constraintTop_toTopOf="parent"/>
    <View
        android:id="@+id/project_middle_line_mask"
        android:background="#ffebef"
        android:visibility="gone"
        android:layout_width="4dp"
        android:layout_height="0dp"
        android:layout_marginTop="3dp"
        android:layout_marginBottom="3dp"
        android:layout_marginStart="-3dp"
        app:layout_constraintBottom_toBottomOf="@+id/project_prefix_tag_view"
        app:layout_constraintStart_toEndOf="@+id/project_prefix_tag_view"
        app:layout_constraintTop_toTopOf="@+id/project_prefix_tag_view"/>
    <cn.damai.trade.newtradeorder.ui.projectdetail.projectdetailnew.view.DashedLineView
        android:id="@+id/project_middle_line"
        android:layout_width="3px"
        android:layout_height="0dp"
        android:layout_marginTop="3dp"
        android:layout_marginBottom="3dp"
        android:layout_marginStart="-3px"
        app:layout_constraintBottom_toBottomOf="@+id/project_prefix_tag_view"
        app:layout_constraintStart_toEndOf="@+id/project_prefix_tag_view"
        app:layout_constraintTop_toTopOf="@+id/project_prefix_tag_view"/>
</androidx.constraintlayout.widget.ConstraintLayout>
