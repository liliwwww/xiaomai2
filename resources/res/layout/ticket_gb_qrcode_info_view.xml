<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.alibaba.pictures.moimage.MoImageView
        android:id="@+id/ticket_gb_qrcode_info_code"
        android:layout_width="160dp"
        android:layout_height="160dp"
        android:layout_marginTop="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0"/>
    <com.taobao.movie.android.commonui.widget.IconFontTextView
        android:textSize="@dimen/ticket_detail_status_iconfont_size"
        android:textColor="@color/tpp_primary_red"
        android:id="@+id/ticket_gb_qrcode_info_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="112dp"
        android:text=""
        android:includeFontPadding="false"
        app:layout_constraintBottom_toBottomOf="@+id/ticket_gb_qrcode_info_code"
        app:layout_constraintLeft_toLeftOf="@+id/ticket_gb_qrcode_info_code"/>
</androidx.constraintlayout.widget.ConstraintLayout>
