<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ImageView
        android:id="@+id/iv_nft_ticket_back_rim"
        android:background="@drawable/ticket_nft_back_rim"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleX="-1"/>
    <cn.damai.uikit.view.HoleCardView
        android:id="@+id/ticklet_ticket_hv_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/ticklet_nft_hole_layout_marginTop"
        android:layout_marginBottom="@dimen/ticklet_nft_hole_layout_marginBottom"
        android:layout_marginStart="@dimen/ticklet_nft_hole_layout_marginStart_end"
        android:layout_marginEnd="@dimen/ticklet_nft_hole_layout_marginStart_end"
        app:holeBackgroundColor="@android:color/transparent"
        app:holeCornerRadius="@dimen/ticklet_nft_hole_corner_radius"
        app:holeScallopRadius="@dimen/ticklet_nft_hole_scallop_radius">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:orientation="vertical"
            android:id="@+id/ticklet_ticket_layout"
            android:background="@drawable/ticket_nft_bg"
            android:paddingTop="@dimen/margin_14dp"
            android:paddingBottom="@dimen/margin_16dp"
            android:clipChildren="false"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <cn.damai.ticklet.view.TickletTicketTipView
                android:id="@+id/ticklet_ticket_rl_tip_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:minHeight="63dp"
                app:layout_constraintTop_toTopOf="parent"
                app:view_type="NFT"/>
            <cn.damai.ticklet.view.TickletTicketMainView
                android:gravity="center"
                android:layout_gravity="center"
                android:id="@+id/ticklet_ticket_main_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/ticklet_ticket_rl_tip_view"
                app:view_type="NFT"/>
            <View
                android:id="@+id/ticklet_ticket_main_middle_holder_view"
                android:background="@color/transparent"
                android:layout_width="@dimen/ticklet_margin_110"
                android:layout_height="8dp"
                app:layout_constraintBottom_toBottomOf="@+id/ticklet_ticket_main_view"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>
            <cn.damai.ticklet.view.TickletTicketStandPortalView
                android:gravity="center"
                android:layout_gravity="center"
                android:id="@+id/ticklet_ticket_stand_portal_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/ticklet_ticket_main_middle_holder_view"
                app:view_type="DEFAULT"/>
            <TextView
                android:gravity="center"
                android:layout_gravity="center_horizontal"
                android:id="@+id/ticket_card_name_cert"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_3dp"
                android:layout_marginBottom="@dimen/margin_23dp"
                app:layout_constraintTop_toBottomOf="@+id/ticklet_ticket_stand_portal_view"
                style="@style/ticklet_111111_13dp"/>
            <cn.damai.ticklet.view.TickletTicketOrderView
                android:gravity="center"
                android:layout_gravity="center"
                android:id="@+id/ticklet_ticket_order_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ticket_card_name_cert"
                app:layout_constraintTop_toBottomOf="@+id/ticket_card_name_cert"
                app:view_type="DEFAULT"/>
            <LinearLayout
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:id="@+id/ll_deatil_seat"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ticklet_ticket_order_view"
                app:layout_constraintTop_toBottomOf="@+id/ticklet_ticket_order_view"/>
            <TextView
                android:gravity="center"
                android:id="@+id/ticket_unique_no_tv"
                android:visibility="visible"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_3dp"
                app:layout_constraintTop_toBottomOf="@+id/ll_deatil_seat"
                style="@style/ticklet_999999_13dp"/>
            <include
                android:id="@+id/ticket_get_model_layout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin_10dp"
                android:layout_marginTop="@dimen/margin_5dp"
                app:layout_constraintRight_toLeftOf="@+id/ticklet_ticket_main_middle_holder_view"
                app:layout_constraintTop_toBottomOf="@+id/ticklet_ticket_stand_portal_view"
                layout="@layout/ticklet_ticket_get_model_layout"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </cn.damai.uikit.view.HoleCardView>
</FrameLayout>
