<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:textSize="12dp"
        android:textColor="#333333"
        android:id="@+id/A_P_C_downgrade_title"
        android:paddingLeft="2dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:textSize="12dp"
        android:textColor="#333333"
        android:id="@+id/A_P_C_downgrade_subtitle"
        android:paddingLeft="2dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:textSize="12dp"
        android:textColor="#333333"
        android:id="@+id/A_P_C_downgrade_name"
        android:paddingLeft="2dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:textSize="12dp"
        android:textColor="#333333"
        android:id="@+id/A_P_C_downgrade_value"
        android:paddingLeft="2dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:textSize="12dp"
        android:textColor="#333333"
        android:id="@+id/A_P_C_downgrade_desc"
        android:paddingLeft="2dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:textSize="12dp"
        android:textColor="#333333"
        android:id="@+id/A_P_C_downgrade_count"
        android:paddingLeft="2dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:textSize="12dp"
        android:textColor="#333333"
        android:id="@+id/A_P_C_downgrade_quantity"
        android:paddingLeft="2dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:textSize="12dp"
        android:textColor="#333333"
        android:id="@+id/A_P_C_downgrade_price"
        android:paddingLeft="2dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
</merge>
