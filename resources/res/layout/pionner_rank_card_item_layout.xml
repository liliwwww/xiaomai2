<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.cardview.widget.CardView
        android:id="@+id/pioneer_super_cd_card_poster"
        android:layout_width="29dp"
        android:layout_height="39dp"
        app:cardCornerRadius="4dp"
        app:cardElevation="0dp"
        app:cardUseCompatPadding="false">
        <com.alibaba.pictures.moimage.MoImageView
            android:id="@+id/pioneer_feed_rank_poster"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="centerCrop"
            app:failureImage="@drawable/home_default_image_bg_gradient_pioneer"
            app:failureImageScaleType="fitXY"
            app:placeholderImage="@drawable/home_default_image_bg_gradient_pioneer"
            app:placeholderImageScaleType="fitXY"
            app:roundingBorderColor="#0d000000"
            app:roundingBorderWidth="0.1dp"/>
        <com.alibaba.pictures.bricks.view.PuHuiTiTextView
            android:textSize="@dimen/margin_13dp"
            android:textColor="@color/bricks_171717"
            android:gravity="center"
            android:id="@+id/pioneer_feed_rank_flag"
            android:background="@drawable/bg_border_corner_rank_num_bg"
            android:layout_width="@dimen/margin_15dp"
            android:layout_height="17dp"
            android:includeFontPadding="false"/>
    </androidx.cardview.widget.CardView>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="@dimen/margin_6dp">
        <TextView
            android:textSize="@dimen/margin_13dp"
            android:textColor="@color/bricks_171717"
            android:id="@+id/pioneer_feed_rank_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxLines="2"
            android:includeFontPadding="false"/>
        <View
            android:layout_gravity="bottom|right"
            android:id="@+id/pioneer_feed_rank_end_cover"
            android:background="@drawable/text_view_rank_end_cover"
            android:visibility="visible"
            android:layout_width="34dp"
            android:layout_height="24dp"/>
    </FrameLayout>
</merge>
