<?xml version="1.0" encoding="utf-8"?>
<com.taobao.movie.android.app.order.ui.widget.TrackerFrameLayout2 xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <View
        android:id="@+id/blank_black"
        android:background="#40000000"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"/>
    <LinearLayout
        android:layout_gravity="bottom"
        android:orientation="vertical"
        android:id="@+id/ll_spring_open_card_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <FrameLayout
            android:gravity="bottom"
            android:orientation="vertical"
            android:background="@drawable/order_page_dialog_head_title_bg"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:layout_width="match_parent"
            android:layout_height="39dp">
            <com.taobao.movie.android.commonui.widget.IconFontTextView
                android:textSize="21dp"
                android:textColor="@color/color_tpp_primary_gray_c8"
                android:layout_gravity="bottom|right"
                android:id="@+id/tv_spring_open_card_cancel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/margin_3"
                android:text="@string/icon_font_close"/>
            <TextView
                android:textSize="18dp"
                android:textStyle="bold"
                android:textColor="@color/color_tpp_primary_main_title"
                android:gravity="center"
                android:layout_gravity="bottom"
                android:id="@+id/tv_spring_open_card_title"
                android:includeFontPadding="false"
                android:layout_alignParentBottom="true"
                style="@style/CommonListItemOrderTitle"/>
        </FrameLayout>
        <FrameLayout
            android:id="@+id/fl_spring_open_card_contain"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <FrameLayout
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:gravity="center"
                android:layout_gravity="bottom"
                android:orientation="vertical"
                android:id="@+id/ll_spring_dialog_button"
                android:background="@drawable/spring_open_card_bottom_bg"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:layout_marginBottom="34dp">
                <LinearLayout
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textSize="11dp"
                        android:textColor="@color/white"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:id="@+id/tv_spring_dialog_button_origin_price"
                        android:paddingRight="4dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:maxLines="1"
                        android:includeFontPadding="false"
                        android:alpha="0.7"/>
                    <TextView
                        android:textSize="16dp"
                        android:textStyle="bold"
                        android:textColor="@color/white"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:id="@+id/tv_spring_dialog_button_desc"
                        android:visibility="visible"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:maxLines="1"
                        android:includeFontPadding="false"/>
                </LinearLayout>
                <TextView
                    android:textSize="10dp"
                    android:textColor="@color/white"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:id="@+id/tv_spring_dialog_button_bottom_desc"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:maxLines="1"
                    android:includeFontPadding="false"
                    android:alpha="0.7"/>
            </LinearLayout>
            <LinearLayout
                android:gravity="center"
                android:layout_gravity="bottom|center_horizontal"
                android:orientation="horizontal"
                android:id="@+id/ll_spring_open_card_dialog_tip"
                android:background="@drawable/spring_open_card_tip"
                android:paddingLeft="12dp"
                android:paddingRight="12dp"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="18dp"
                android:layout_marginBottom="76dp">
                <TextView
                    android:textSize="11dp"
                    android:textColor="#fff12140"
                    android:gravity="center"
                    android:id="@+id/tv_spring_open_card_dialog_tag"
                    android:paddingRight="2dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <TextView
                    android:textSize="11dp"
                    android:textColor="#ff743b0c"
                    android:gravity="center"
                    android:id="@+id/tv_spring_open_card_dialog_tip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
            <View
                android:layout_gravity="center_horizontal"
                android:id="@+id/v_spring_open_card_dialog_tip_bottom"
                android:background="@drawable/spring_open_card_tip_bottom"
                android:visibility="gone"
                android:layout_width="@dimen/padding_10dp"
                android:layout_height="@dimen/padding_10dp"
                android:layout_marginTop="17.5dp"/>
        </FrameLayout>
    </LinearLayout>
</com.taobao.movie.android.app.order.ui.widget.TrackerFrameLayout2>
