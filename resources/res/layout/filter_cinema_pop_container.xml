<?xml version="1.0" encoding="utf-8"?>
<com.taobao.movie.android.app.order.ui.widget.TrackerFrameLayout2 xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="vertical"
        android:background="#99000000"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <androidx.core.widget.NestedScrollView
            android:background="@color/white"
            android:scrollbars="none"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="4">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:background="@color/white"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="14dp"
                    android:textColor="@color/color_tpp_primary_subtitle"
                    android:id="@+id/start_note_txt"
                    android:paddingTop="15dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:text="开场时间: "
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                    android:textSize="14dp"
                    android:textStyle="bold"
                    android:textColor="@color/filter_cinema_text_color"
                    android:id="@+id/time_area"
                    android:paddingTop="15dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="12：30以后"
                    app:layout_constraintBaseline_toBaselineOf="@+id/start_note_txt"
                    app:layout_constraintLeft_toRightOf="@+id/start_note_txt"/>
                <com.taobao.movie.android.commonui.rangseekbar.RangeSeekBar
                    android:id="@+id/time_seekbar"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:rsb_indicator_drawable="@drawable/time_filter_bg"
                    app:rsb_indicator_height="26dp"
                    app:rsb_indicator_margin="15dp"
                    app:rsb_indicator_margin_right="8dp"
                    app:rsb_indicator_padding_right="6dp"
                    app:rsb_indicator_padding_top="12dp"
                    app:rsb_indicator_radius="90dp"
                    app:rsb_indicator_show_mode="showWhenTouch"
                    app:rsb_indicator_text_color="@color/white"
                    app:rsb_indicator_text_size="12dp"
                    app:rsb_indicator_width="55dp"
                    app:rsb_max="48"
                    app:rsb_min="0"
                    app:rsb_min_interval="4"
                    app:rsb_mode="range"
                    app:rsb_progress_color="@color/filter_cinema_text_color"
                    app:rsb_progress_default_color="@color/deliver_not_select"
                    app:rsb_progress_height="2dp"
                    app:rsb_thumb_drawable="@drawable/filter_thumb"
                    app:rsb_thumb_height="34dp"
                    app:rsb_thumb_width="34dp"/>
                <TextView
                    android:textSize="12dp"
                    android:id="@+id/start_time_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="6dp"
                    android:text="00:00"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/time_seekbar"/>
                <TextView
                    android:textSize="12dp"
                    android:id="@+id/end_time_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:layout_marginRight="15dp"
                    android:text="24:00"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/time_seekbar"/>
                <View
                    android:id="@+id/first_line"
                    android:background="@color/color_tpp_primary_divider_light"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/space_line_height"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="15dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/start_time_txt"/>
                <TextView
                    android:textSize="14dp"
                    android:textColor="@color/color_tpp_primary_subtitle"
                    android:id="@+id/privileges_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="15dp"
                    android:text="会员特权"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/first_line"/>
                <TextView
                    android:textSize="12dp"
                    android:textColor="@color/color_tpp_primary_subtitle"
                    android:id="@+id/privileges_more_txt"
                    android:paddingRight="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="展开"
                    app:layout_constraintBaseline_toBaselineOf="@+id/privileges_txt"
                    app:layout_constraintRight_toLeftOf="@+id/privileges_arrow"/>
                <com.taobao.movie.android.commonui.widget.MIconfontTextView
                    android:textSize="10dp"
                    android:textColor="@color/color_tpp_primary_subtitle"
                    android:id="@+id/privileges_arrow"
                    android:paddingRight="15dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/iconf_portrait_video_open_index_arrow"
                    app:layout_constraintBaseline_toBaselineOf="@+id/privileges_txt"
                    app:layout_constraintRight_toRightOf="parent"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/privileges_recycle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="15dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/privileges_txt"/>
                <View
                    android:id="@+id/privileges_line"
                    android:background="@color/color_tpp_primary_divider_light"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="15dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/privileges_recycle"/>
                <TextView
                    android:textSize="14dp"
                    android:textColor="@color/color_tpp_primary_subtitle"
                    android:id="@+id/cinema_type_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="15dp"
                    android:text="放映影厅"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/privileges_line"/>
                <TextView
                    android:textSize="12dp"
                    android:textColor="@color/color_tpp_primary_subtitle"
                    android:id="@+id/cinema_more_txt"
                    android:paddingRight="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="展开"
                    app:layout_constraintBaseline_toBaselineOf="@+id/cinema_type_txt"
                    app:layout_constraintRight_toLeftOf="@+id/cinema_arrow"/>
                <com.taobao.movie.android.commonui.widget.MIconfontTextView
                    android:textSize="10dp"
                    android:textColor="@color/color_tpp_primary_subtitle"
                    android:id="@+id/cinema_arrow"
                    android:paddingRight="15dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/iconf_portrait_video_open_index_arrow"
                    app:layout_constraintBaseline_toBaselineOf="@+id/cinema_type_txt"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/first_line"/>
                <com.taobao.movie.android.commonui.widget.MIconfontTextView
                    android:textSize="12dp"
                    android:textColor="@color/color_tpp_primary_subtitle"
                    android:id="@+id/introduce_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="19dp"
                    android:text="@string/iconf_question_mark"
                    app:layout_constraintLeft_toRightOf="@+id/cinema_type_txt"
                    app:layout_constraintTop_toBottomOf="@+id/privileges_line"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/cinema_type_recycle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="15dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cinema_type_txt"/>
                <View
                    android:id="@+id/second_line"
                    android:background="@color/color_tpp_primary_divider_light"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/space_line_height"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="15dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cinema_type_recycle"/>
                <TextView
                    android:textSize="14dp"
                    android:textColor="@color/color_tpp_primary_subtitle"
                    android:id="@+id/cinema_service_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="15dp"
                    android:text="影院服务"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/second_line"/>
                <TextView
                    android:textSize="12dp"
                    android:textColor="@color/color_tpp_primary_subtitle"
                    android:id="@+id/cinema_service_more_txt"
                    android:paddingRight="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="展开"
                    app:layout_constraintBaseline_toBaselineOf="@+id/cinema_service_txt"
                    app:layout_constraintRight_toLeftOf="@+id/cinema_service_arrow"/>
                <com.taobao.movie.android.commonui.widget.MIconfontTextView
                    android:textSize="10dp"
                    android:textColor="@color/color_tpp_primary_subtitle"
                    android:id="@+id/cinema_service_arrow"
                    android:paddingRight="15dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/iconf_portrait_video_open_index_arrow"
                    app:layout_constraintBaseline_toBaselineOf="@+id/cinema_service_txt"
                    app:layout_constraintRight_toRightOf="parent"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/cinema_service_recycle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="15dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cinema_service_txt"/>
                <View
                    android:id="@+id/third_line"
                    android:background="@color/color_tpp_primary_divider_light"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/space_line_height"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="15dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cinema_service_recycle"/>
                <TextView
                    android:textSize="14dp"
                    android:textColor="@color/color_tpp_primary_subtitle"
                    android:id="@+id/cinema_language_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="15dp"
                    android:text="影片语言"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/third_line"/>
                <TextView
                    android:textSize="12dp"
                    android:textColor="@color/color_tpp_primary_subtitle"
                    android:id="@+id/language_more_txt"
                    android:paddingRight="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="展开"
                    app:layout_constraintBaseline_toBaselineOf="@+id/cinema_language_txt"
                    app:layout_constraintRight_toLeftOf="@+id/language_arrow"/>
                <com.taobao.movie.android.commonui.widget.MIconfontTextView
                    android:textSize="10dp"
                    android:textColor="@color/color_tpp_primary_subtitle"
                    android:id="@+id/language_arrow"
                    android:paddingRight="15dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/iconf_portrait_video_open_index_arrow"
                    app:layout_constraintBaseline_toBaselineOf="@+id/cinema_language_txt"
                    app:layout_constraintRight_toRightOf="parent"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/cinema_language_recycle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="15dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cinema_language_txt"/>
                <View
                    android:id="@+id/forth_line"
                    android:background="@color/color_tpp_primary_divider_light"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/space_line_height"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="15dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cinema_language_recycle"/>
                <TextView
                    android:textSize="14dp"
                    android:textColor="@color/color_tpp_primary_subtitle"
                    android:id="@+id/film_version_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="15dp"
                    android:text="影片版本"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/forth_line"/>
                <TextView
                    android:textSize="12dp"
                    android:textColor="@color/color_tpp_primary_subtitle"
                    android:id="@+id/version_more_txt"
                    android:paddingRight="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="展开"
                    app:layout_constraintBaseline_toBaselineOf="@+id/film_version_txt"
                    app:layout_constraintRight_toLeftOf="@+id/version_arrow"/>
                <com.taobao.movie.android.commonui.widget.MIconfontTextView
                    android:textSize="10dp"
                    android:textColor="@color/color_tpp_primary_subtitle"
                    android:id="@+id/version_arrow"
                    android:paddingRight="15dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/iconf_portrait_video_open_index_arrow"
                    app:layout_constraintBaseline_toBaselineOf="@+id/film_version_txt"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/forth_line"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/film_version_recycle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="15dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/film_version_txt"/>
                <View
                    android:id="@+id/version_line"
                    android:background="@color/color_tpp_primary_divider_light"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/space_line_height"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="15dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/film_version_recycle"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.core.widget.NestedScrollView>
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <View
                android:layout_gravity="bottom"
                android:id="@+id/fill_bg_layout"
                android:background="#99000000"
                android:clickable="false"
                android:layout_width="match_parent"
                android:layout_height="6dp"/>
            <ViewStub
                android:id="@+id/stub_filter_brand_pop_container"
                android:layout="@layout/cinema_list_filter_pop_confirm_cancel_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <ViewStub
                android:id="@+id/stub_filter_brand_pop_container_pioneer"
                android:layout="@layout/cinema_list_filter_pop_confirm_cancel_pioneer_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
        </FrameLayout>
        <View
            android:id="@+id/bg_layout"
            android:background="#99000000"
            android:clickable="false"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"/>
    </LinearLayout>
</com.taobao.movie.android.app.order.ui.widget.TrackerFrameLayout2>
