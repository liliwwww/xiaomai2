<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.airbnb.lottie.LottieAnimationView
        android:layout_gravity="top|right"
        android:id="@+id/member_lottie_strategy"
        android:visibility="gone"
        android:layout_width="110dp"
        android:layout_height="55dp"
        android:layout_marginBottom="-20dp"
        android:scaleType="fitXY"
        android:layout_marginStart="@dimen/margin_12dp"
        android:layout_marginEnd="@dimen/margin_9dp"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"/>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/margin_9dp"
        android:layout_marginEnd="@dimen/margin_9dp">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/project_timer_layout"
            android:background="?attr/bgVipBuyVipSellBg"
            android:paddingLeft="9dp"
            android:paddingTop="9dp"
            android:paddingRight="9dp"
            android:layout_width="0dp"
            android:layout_height="60dp"
            android:layout_weight="1">
            <cn.damai.uikit.number.DMDigitTextView
                android:textSize="16dp"
                android:textColor="?attr/colorFF4361to171717"
                android:gravity="center"
                android:id="@+id/prompt_timer_day"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:textSize="12dp"
                android:textColor="#4d4d56"
                android:ellipsize="end"
                android:gravity="center"
                android:id="@+id/prompt_timer_day_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="天"
                android:maxLines="1"
                app:layout_constraintBottom_toBottomOf="@+id/prompt_timer_day_hours"
                app:layout_constraintLeft_toRightOf="@+id/prompt_timer_day"
                app:layout_constraintTop_toTopOf="@+id/prompt_timer_day_hours"/>
            <cn.damai.uikit.number.DMDigitTextView
                android:textSize="16dp"
                android:textColor="?attr/colorFF4361to171717"
                android:ellipsize="end"
                android:gravity="center"
                android:id="@+id/prompt_timer_day_hours"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="6dp"
                android:maxLines="1"
                app:layout_constraintLeft_toRightOf="@+id/prompt_timer_day_text"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:textSize="11dp"
                android:textColor="?attr/bgVipBuyRemindAddTvColor"
                android:ellipsize="end"
                android:gravity="center"
                android:id="@+id/prompt_timer_reminder"
                android:background="?attr/bgVipBuyRemindAddBg"
                android:paddingLeft="@dimen/margin_6dp"
                android:paddingRight="@dimen/margin_6dp"
                android:layout_width="wrap_content"
                android:layout_height="21dp"
                android:layout_marginBottom="7.5dp"
                android:text="添加提醒"
                android:maxLines="1"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>
            <TextView
                android:textSize="@dimen/margin_12dp"
                android:textColor="@color/color_4D4D56"
                android:ellipsize="end"
                android:gravity="center_vertical"
                android:id="@+id/prompt_timer_desc"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:maxLines="1"
                android:layout_marginEnd="3dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toLeftOf="@+id/prompt_timer_reminder"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <android.widget.Space
            android:id="@+id/space"
            android:layout_width="6dp"
            android:layout_height="1dp"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/prompt_layout"
            android:background="@drawable/bg_member_prompt_vip_buy_bg"
            android:paddingLeft="9dp"
            android:paddingTop="9dp"
            android:paddingRight="9dp"
            android:layout_width="0dp"
            android:layout_height="60dp"
            android:layout_weight="1">
            <com.alibaba.pictures.moimage.MoImageView
                android:id="@+id/prompt_vip_icon"
                android:layout_width="62dp"
                android:layout_height="@dimen/margin_18dp"
                android:src="?attr/bgVipBuyLogo"
                android:scaleType="fitXY"
                app:failureImage="?attr/bgVipBuyLogo"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:placeholderImage="?attr/bgVipBuyLogo"/>
            <TextView
                android:textSize="@dimen/margin_14dp"
                android:textColor="?attr/color874953to171717"
                android:gravity="center"
                android:id="@+id/prompt_timer_exchange_type"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin_3dp"
                android:text="专享购"
                app:layout_constraintLeft_toRightOf="@+id/prompt_vip_icon"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:textSize="@dimen/margin_11dp"
                android:textColor="?attr/bgDetailColorWhite1C1C1C"
                android:gravity="center"
                android:id="@+id/prompt_exchange_action"
                android:background="@drawable/bg_f5ac8b_ff886e_corner11"
                android:paddingLeft="@dimen/margin_6dp"
                android:paddingRight="@dimen/margin_6dp"
                android:layout_width="wrap_content"
                android:layout_height="21dp"
                android:layout_marginBottom="7.5dp"
                android:includeFontPadding="false"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>
            <TextView
                android:textSize="@dimen/margin_12dp"
                android:textColor="@color/color_4D4D56"
                android:ellipsize="end"
                android:gravity="start|center_vertical"
                android:id="@+id/prompt_desc"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:maxLines="1"
                android:layout_marginEnd="2dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toRightOf="@+id/prompt_exchange_desc_icon"
                app:layout_constraintRight_toLeftOf="@+id/prompt_exchange_action"/>
            <ImageView
                android:id="@+id/prompt_exchange_desc_icon"
                android:layout_width="12dp"
                android:layout_height="12dp"
                android:layout_marginRight="@dimen/margin_3dp"
                android:src="@drawable/trade_member_lightning_icon"
                android:scaleType="fitXY"
                app:layout_constraintBottom_toBottomOf="@+id/prompt_desc"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="@+id/prompt_desc"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>
</LinearLayout>
