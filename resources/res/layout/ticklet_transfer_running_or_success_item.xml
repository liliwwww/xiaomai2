<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:gravity="center_vertical"
    android:orientation="vertical"
    android:background="@color/white"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:minHeight="80dp">
    <LinearLayout
        android:orientation="horizontal"
        android:id="@+id/ticklet_transfer_ll_phone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/margin_19dp"
        android:layout_marginTop="@dimen/margin_16dp"
        android:layout_marginRight="@dimen/margin_18dp">
        <TextView
            android:id="@+id/ticklet_transfer_people_tips"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/ticklet_transfer_person_tip"
            style="@style/ticklet_4a4a4a_14dp"/>
        <TextView
            android:id="@+id/ticklet_transfer_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_14dp"
            android:layout_weight="1"
            style="@style/ticklet_4a4a4a_14dp"/>
        <TextView
            android:textSize="@dimen/margin_13dp"
            android:textColor="#0e0e0e"
            android:gravity="center_horizontal"
            android:id="@+id/ticklet_transfer_cancel"
            android:background="@drawable/ticklet_transfer_bg_running_cancel_value"
            android:paddingLeft="26dp"
            android:paddingTop="@dimen/margin_6dp"
            android:paddingRight="26dp"
            android:paddingBottom="@dimen/margin_6dp"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_14dp"
            android:text="撤销"/>
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/margin_19dp"
        android:layout_marginTop="@dimen/margin_12dp"
        android:layout_marginRight="@dimen/margin_18dp">
        <TextView
            android:id="@+id/ticklet_transfer_seat_tips"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="座位号"
            style="@style/ticklet_aaaaaa_14dp"/>
        <TextView
            android:id="@+id/ticklet_transfer_seat_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_14dp"
            style="@style/ticklet_aaaaaa_14dp"/>
    </LinearLayout>
    <LinearLayout
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:id="@+id/ticklet_ll_transfer_tn"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/margin_19dp"
        android:layout_marginTop="@dimen/margin_6dp"
        android:layout_marginRight="@dimen/margin_18dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="票单号"
            style="@style/ticklet_aaaaaa_14dp"/>
        <TextView
            android:id="@+id/ticklet_transfer_tn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_14dp"
            style="@style/ticklet_aaaaaa_14dp"/>
    </LinearLayout>
    <LinearLayout
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:id="@+id/ticklet_transfer_ll_running_time"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/margin_19dp"
        android:layout_marginTop="@dimen/margin_6dp"
        android:layout_marginRight="@dimen/margin_18dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="转送中"
            style="@style/ticklet_aaaaaa_14dp"/>
        <TextView
            android:id="@+id/ticklet_transfer_remainingTime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_14dp"
            style="@style/ticklet_ff2d79_14dp"/>
    </LinearLayout>
    <LinearLayout
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:id="@+id/ticklet_transfer_ll_accept_time"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/margin_19dp"
        android:layout_marginTop="@dimen/margin_6dp"
        android:layout_marginRight="@dimen/margin_18dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="时    间"
            style="@style/ticklet_aaaaaa_14dp"/>
        <TextView
            android:id="@+id/ticklet_transfer_accept_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_14dp"
            style="@style/ticklet_aaaaaa_14dp"/>
    </LinearLayout>
    <View
        android:id="@+id/ticklet_transfer_running_success_line"
        android:background="@color/color_E7E7E7"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/margin_18dp"
        android:layout_marginTop="@dimen/margin_18dp"
        android:layout_marginRight="@dimen/margin_18dp"/>
</LinearLayout>
