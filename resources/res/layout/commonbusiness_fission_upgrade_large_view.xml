<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_gravity="center"
    android:id="@+id/root"
    android:paddingLeft="@dimen/margin_6dp"
    android:paddingTop="@dimen/margin_6dp"
    android:paddingRight="@dimen/margin_6dp"
    android:paddingBottom="32dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ImageView
        android:id="@+id/fission_upgrade_image"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scaleType="centerCrop"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <View
        android:id="@+id/fission_upgrade_holder"
        android:background="@color/transparent"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        app:layout_constraintBottom_toBottomOf="@+id/fission_upgrade_image"/>
    <ImageView
        android:id="@+id/fission_upgrade_sep_line"
        android:background="@color/transparent"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:src="@drawable/commonbusiness_fission_sep_new"
        android:scaleType="fitXY"
        app:layout_constraintTop_toTopOf="@+id/fission_upgrade_holder"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/fission_upgrade_content_info"
        android:background="@drawable/bg_fissionup_grade_info_background"
        android:paddingLeft="@dimen/margin_21dp"
        android:paddingBottom="@dimen/margin_21dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/fission_upgrade_sep_line">
        <TextView
            android:textSize="@dimen/margin_16dp"
            android:textColor="@color/color_000000"
            android:ellipsize="end"
            android:id="@+id/fission_upgrade_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:maxLines="1"
            app:layout_constraintLeft_toLeftOf="@+id/fission_upgrade_content_info"
            app:layout_constraintRight_toRightOf="@+id/fission_upgrade_content_info"
            app:layout_constraintTop_toTopOf="@+id/fission_upgrade_content_info"/>
        <TextView
            android:textSize="@dimen/margin_12dp"
            android:textColor="@color/color_9C9CA5"
            android:ellipsize="end"
            android:id="@+id/fission_upgrade_time_des"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_21dp"
            android:text="时间Time"
            android:maxLines="1"
            app:layout_constraintLeft_toLeftOf="@+id/fission_upgrade_content_info"
            app:layout_constraintTop_toBottomOf="@+id/fission_upgrade_name"/>
        <TextView
            android:textSize="@dimen/margin_14dp"
            android:textColor="@color/color_000000"
            android:ellipsize="end"
            android:id="@+id/fission_upgrade_time"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_5dp"
            android:layout_marginRight="@dimen/margin_10dp"
            android:maxLines="1"
            app:layout_constraintLeft_toLeftOf="@+id/fission_upgrade_content_info"
            app:layout_constraintRight_toLeftOf="@+id/fission_upgrade_venue"
            app:layout_constraintTop_toBottomOf="@+id/fission_upgrade_time_des"/>
        <TextView
            android:textSize="@dimen/margin_12dp"
            android:textColor="@color/color_9C9CA5"
            android:ellipsize="end"
            android:id="@+id/fission_upgrade_seat_des"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_12dp"
            android:text="座位Seat"
            android:maxLines="1"
            app:layout_constraintLeft_toLeftOf="@+id/fission_upgrade_content_info"
            app:layout_constraintTop_toBottomOf="@+id/fission_upgrade_time"/>
        <TextView
            android:textSize="@dimen/margin_14dp"
            android:textColor="@color/color_000000"
            android:ellipsize="end"
            android:id="@+id/fission_upgrade_seat"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_3dp"
            android:layout_marginRight="@dimen/margin_10dp"
            android:lines="1"
            app:layout_constraintLeft_toLeftOf="@+id/fission_upgrade_content_info"
            app:layout_constraintRight_toLeftOf="@+id/fission_upgrade_qrcode"
            app:layout_constraintTop_toBottomOf="@+id/fission_upgrade_seat_des"/>
        <cn.damai.uikit.view.RoundImageView
            android:id="@+id/fission_upgrade_user_image"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginTop="@dimen/margin_26dp"
            android:src="@drawable/uikit_user_default_icon"
            app:layout_constraintLeft_toLeftOf="@+id/fission_upgrade_content_info"
            app:layout_constraintTop_toBottomOf="@+id/fission_upgrade_seat"/>
        <TextView
            android:textSize="@dimen/margin_10dp"
            android:textColor="@color/color_000000"
            android:ellipsize="end"
            android:id="@+id/fission_upgrade_user_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_3dp"
            android:layout_marginTop="@dimen/margin_3dp"
            android:lines="1"
            app:layout_constraintLeft_toRightOf="@+id/fission_upgrade_user_image"
            app:layout_constraintTop_toTopOf="@+id/fission_upgrade_user_image"/>
        <TextView
            android:textSize="@dimen/margin_12dp"
            android:textColor="@color/color_9C9CA5"
            android:ellipsize="end"
            android:id="@+id/fission_upgrade_chickensoup"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_6dp"
            android:layout_marginRight="@dimen/margin_10dp"
            android:lines="1"
            app:layout_constraintLeft_toLeftOf="@+id/fission_upgrade_content_info"
            app:layout_constraintRight_toLeftOf="@+id/fission_upgrade_qrcode"
            app:layout_constraintTop_toBottomOf="@+id/fission_upgrade_user_image"/>
        <TextView
            android:textSize="@dimen/margin_12dp"
            android:textColor="@color/color_9C9CA5"
            android:ellipsize="end"
            android:id="@+id/fission_upgrade_venue_des"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:text="场馆Venue"
            android:lines="1"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/fission_upgrade_time_des"/>
        <TextView
            android:textSize="@dimen/margin_14dp"
            android:textColor="@color/color_000000"
            android:ellipsize="end"
            android:id="@+id/fission_upgrade_venue"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_3dp"
            android:layout_marginRight="@dimen/margin_21dp"
            android:lines="1"
            app:layout_constraintLeft_toLeftOf="@+id/fission_upgrade_venue_des"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/fission_upgrade_venue_des"/>
        <TextView
            android:textSize="@dimen/margin_12dp"
            android:textColor="@color/color_9C9CA5"
            android:ellipsize="end"
            android:id="@+id/fission_upgrade_entry_des"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_12dp"
            android:text="入口Enter"
            android:lines="1"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintLeft_toLeftOf="@+id/fission_upgrade_venue_des"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/fission_upgrade_venue"/>
        <ImageView
            android:id="@+id/fission_upgrade_qrcode"
            android:visibility="gone"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_marginTop="@dimen/margin_4dp"
            app:layout_constraintLeft_toLeftOf="@+id/fission_upgrade_entry_des"
            app:layout_constraintTop_toBottomOf="@+id/fission_upgrade_entry_des"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</merge>
