<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#f2f3f4"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include
        android:id="@+id/mine_privacy_setting_title_layout"
        layout="@layout/mine_title_header"/>
    <ScrollView
        android:scrollbars="none"
        android:fadingEdge="none"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:orientation="vertical"
                android:id="@+id/privacy_view_when_login"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <View
                    android:id="@+id/split1"
                    android:background="#f2f3f4"
                    android:layout_width="match_parent"
                    android:layout_height="10dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <LinearLayout
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:id="@+id/ll_show_dna"
                    android:background="@color/white"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/split1">
                    <TextView
                        android:textSize="12dp"
                        android:textColor="@color/color_333333"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_16dp"
                        android:text="@string/mine_privacy_setting_dna_tips"
                        android:layout_weight="1"/>
                    <cn.damai.uikit.switchbutton.SwitchButton
                        android:id="@+id/switch_dna"
                        android:layout_width="43dp"
                        android:layout_height="26dp"
                        android:layout_marginRight="16dp"
                        android:checked="false"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        app:kswBackColor="@drawable/bg_setting_recommend_switch_btn"
                        app:kswThumbColor="@color/color_ffffff"
                        app:kswThumbHeight="25dp"
                        app:kswThumbWidth="25dp"/>
                </LinearLayout>
                <TextView
                    android:id="@+id/split2"
                    android:background="#f2f2f2"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/ll_show_dna"/>
                <cn.damai.uikit.iconfont.DMIconFontTextView
                    android:textSize="12dp"
                    android:textColor="@color/color_AAAAAA"
                    android:id="@+id/dtv_dna_check"
                    android:paddingTop="@dimen/margin_14dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/iconfont_gouxuanweixuanzhong28"
                    android:paddingStart="16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/split2"/>
                <TextView
                    android:textSize="12dp"
                    android:textColor="@color/color_AAAAAA"
                    android:id="@+id/tv_dna_content"
                    android:paddingTop="@dimen/margin_12dp"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/mine_privacy_setting_dna_content_tips_default"
                    android:paddingStart="6dp"
                    android:paddingEnd="10dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/dtv_dna_check"
                    app:layout_constraintTop_toBottomOf="@+id/split2"/>
                <cn.damai.uikit.iconfont.DMIconFontTextView
                    android:textSize="12dp"
                    android:textColor="#ff2869"
                    android:id="@+id/dtv_dna_error_icon"
                    android:paddingTop="@dimen/margin_8dp"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/iconfont_tixingxian_"
                    android:paddingStart="16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_dna_content"/>
                <TextView
                    android:textSize="12dp"
                    android:textColor="#ff2869"
                    android:id="@+id/tv_dna_error"
                    android:paddingTop="@dimen/margin_6dp"
                    android:visibility="gone"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/mine_privacy_setting_dna_content_tips_error"
                    android:paddingStart="6dp"
                    android:paddingEnd="10dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/dtv_dna_check"
                    app:layout_constraintTop_toBottomOf="@+id/tv_dna_content"/>
                <TextView
                    android:id="@+id/split3"
                    android:background="#f2f2f2"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="@dimen/margin_18dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_dna_error"/>
                <LinearLayout
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:id="@+id/rl_recommend_switch"
                    android:background="@color/white"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/split3">
                    <TextView
                        android:textSize="12dp"
                        android:textColor="@color/color_333333"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="12dp"
                        android:text="@string/mine_privacy_setting_recommend_content_tips"
                        android:layout_weight="1"/>
                    <cn.damai.uikit.switchbutton.SwitchButton
                        android:id="@+id/recommend_switch"
                        android:layout_width="43dp"
                        android:layout_height="26dp"
                        android:layout_marginRight="16dp"
                        android:checked="true"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        app:kswBackColor="@drawable/bg_setting_recommend_switch_btn"
                        app:kswThumbColor="@color/color_ffffff"
                        app:kswThumbHeight="25dp"
                        app:kswThumbWidth="25dp"/>
                </LinearLayout>
                <TextView
                    android:id="@+id/split4"
                    android:background="#f2f2f2"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/rl_recommend_switch"/>
                <TextView
                    android:textSize="@dimen/margin_12dp"
                    android:textColor="@color/color_AAAAAA"
                    android:id="@+id/tv_recommend_content"
                    android:paddingLeft="16dp"
                    android:paddingTop="@dimen/margin_12dp"
                    android:paddingRight="10dp"
                    android:paddingBottom="@dimen/margin_18dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/mine_privacy_setting_recommend_tips"
                    android:lineSpacingExtra="4dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/split4"/>
                <LinearLayout
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:id="@+id/rl_ai_search_switch"
                    android:background="@color/white"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_recommend_content">
                    <TextView
                        android:textSize="12dp"
                        android:textColor="@color/color_333333"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="12dp"
                        android:text="@string/mine_privacy_setting_ai_search_content_tips"
                        android:layout_weight="1"/>
                    <cn.damai.uikit.switchbutton.SwitchButton
                        android:id="@+id/ai_search_switch"
                        android:layout_width="43dp"
                        android:layout_height="26dp"
                        android:layout_marginRight="16dp"
                        android:checked="true"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        app:kswBackColor="@drawable/bg_setting_recommend_switch_btn"
                        app:kswThumbColor="@color/color_ffffff"
                        app:kswThumbHeight="25dp"
                        app:kswThumbWidth="25dp"/>
                </LinearLayout>
                <TextView
                    android:id="@+id/split7"
                    android:background="#f2f2f2"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/rl_ai_search_switch"/>
                <TextView
                    android:textSize="@dimen/margin_12dp"
                    android:textColor="@color/color_AAAAAA"
                    android:id="@+id/tv_ai_search_content"
                    android:paddingLeft="16dp"
                    android:paddingTop="@dimen/margin_12dp"
                    android:paddingRight="10dp"
                    android:paddingBottom="@dimen/margin_18dp"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/mine_privacy_setting_ai_search_tips"
                    android:lineSpacingExtra="4dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/split7"/>
                <TextView
                    android:id="@+id/split5"
                    android:background="#f2f2f2"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_ai_search_content"/>
                <LinearLayout
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:id="@+id/rl_see_switch"
                    android:background="@color/white"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/split5">
                    <TextView
                        android:textSize="12dp"
                        android:textColor="@color/color_333333"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="12dp"
                        android:text="@string/mine_privacy_setting_see_tips"
                        android:layout_weight="1"/>
                    <cn.damai.uikit.switchbutton.SwitchButton
                        android:id="@+id/see_switch"
                        android:layout_width="43dp"
                        android:layout_height="26dp"
                        android:layout_marginRight="16dp"
                        android:checked="false"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        app:kswBackColor="@drawable/bg_setting_recommend_switch_btn"
                        app:kswThumbColor="@color/color_ffffff"
                        app:kswThumbHeight="25dp"
                        app:kswThumbWidth="25dp"/>
                </LinearLayout>
                <TextView
                    android:id="@+id/split6"
                    android:background="#f2f2f2"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/rl_see_switch"/>
                <TextView
                    android:textSize="@dimen/margin_12dp"
                    android:textColor="@color/color_AAAAAA"
                    android:id="@+id/tv_see_content"
                    android:paddingLeft="16dp"
                    android:paddingTop="@dimen/margin_12dp"
                    android:paddingRight="10dp"
                    android:paddingBottom="@dimen/margin_18dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/mine_privacy_setting_see_content_tips"
                    android:lineSpacingExtra="4dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/split6"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:id="@+id/rl_recommend_switch_when_not_login"
                android:background="@color/white"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_marginTop="@dimen/resource_size_10">
                <TextView
                    android:textSize="12dp"
                    android:textColor="@color/color_333333"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12dp"
                    android:text="@string/mine_privacy_setting_recommend_content_tips"
                    android:layout_weight="1"/>
                <cn.damai.uikit.switchbutton.SwitchButton
                    android:id="@+id/recommend_switch_when_not_login"
                    android:layout_width="43dp"
                    android:layout_height="26dp"
                    android:layout_marginRight="16dp"
                    android:checked="true"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    app:kswBackColor="@drawable/bg_setting_recommend_switch_btn"
                    app:kswThumbColor="@color/color_ffffff"
                    app:kswThumbHeight="25dp"
                    app:kswThumbWidth="25dp"/>
            </LinearLayout>
            <TextView
                android:background="#f2f2f2"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/rl_recommend_switch"/>
            <TextView
                android:textSize="@dimen/margin_12dp"
                android:textColor="@color/color_AAAAAA"
                android:id="@+id/rl_recommend_content_when_not_login"
                android:paddingLeft="16dp"
                android:paddingTop="@dimen/margin_12dp"
                android:paddingRight="10dp"
                android:paddingBottom="@dimen/margin_18dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/mine_privacy_setting_recommend_tips"
                android:lineSpacingExtra="4dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/split4"/>
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:background="@color/white"
                android:visibility="visible"
                android:layout_width="match_parent"
                android:layout_height="55dp">
                <TextView
                    android:textSize="12dp"
                    android:textColor="@color/color_333333"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12dp"
                    android:text="@string/mine_privacy_setting_allow_clipboard"
                    android:layout_weight="1"/>
                <cn.damai.uikit.switchbutton.SwitchButton
                    android:id="@+id/clipboard_switch"
                    android:layout_width="43dp"
                    android:layout_height="26dp"
                    android:layout_marginRight="16dp"
                    android:checked="true"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    app:kswBackColor="@drawable/bg_setting_recommend_switch_btn"
                    app:kswThumbColor="@color/color_ffffff"
                    app:kswThumbHeight="25dp"
                    app:kswThumbWidth="25dp"/>
            </LinearLayout>
            <TextView
                android:textSize="@dimen/margin_12dp"
                android:textColor="@color/color_AAAAAA"
                android:paddingLeft="16dp"
                android:paddingTop="@dimen/margin_12dp"
                android:paddingRight="10dp"
                android:paddingBottom="@dimen/margin_18dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/mine_privacy_clip_api_tips"
                android:lineSpacingExtra="4dp"/>
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:id="@+id/ll_download_user_info"
                android:background="@color/white"
                android:visibility="visible"
                android:layout_width="match_parent"
                android:layout_height="55dp">
                <TextView
                    android:textSize="12dp"
                    android:textColor="@color/color_333333"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12dp"
                    android:text="@string/mine_download_user_info"
                    android:layout_weight="1"/>
                <cn.damai.uikit.iconfont.DMIconFontTextView
                    android:textSize="12dp"
                    android:textColor="@color/color_DDDDDD"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="18dp"
                    android:text="@string/iconfont_youjiantou12"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
            </LinearLayout>
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:background="@color/white"
                android:visibility="visible"
                android:layout_width="match_parent"
                android:layout_height="55dp">
                <TextView
                    android:textSize="12dp"
                    android:textColor="@color/color_333333"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12dp"
                    android:text="@string/mine_privacy_setting_allow_recommend_ad_title"
                    android:layout_weight="1"/>
                <cn.damai.uikit.switchbutton.SwitchButton
                    android:id="@+id/recommend_ad_switch"
                    android:layout_width="43dp"
                    android:layout_height="26dp"
                    android:layout_marginRight="16dp"
                    android:checked="true"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    app:kswBackColor="@drawable/bg_setting_recommend_switch_btn"
                    app:kswThumbColor="@color/color_ffffff"
                    app:kswThumbHeight="25dp"
                    app:kswThumbWidth="25dp"/>
            </LinearLayout>
            <TextView
                android:textSize="@dimen/margin_12dp"
                android:textColor="@color/color_AAAAAA"
                android:paddingLeft="16dp"
                android:paddingTop="@dimen/margin_12dp"
                android:paddingRight="10dp"
                android:paddingBottom="@dimen/margin_18dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/mine_privacy_setting_allow_recommend_ad_desc"
                android:lineSpacingExtra="4dp"/>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
