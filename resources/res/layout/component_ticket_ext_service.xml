<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:gravity="center_vertical"
        android:orientation="vertical"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:paddingStart="@dimen/resource_size_12"
        android:paddingEnd="@dimen/resource_size_12">
        <LinearLayout
            android:gravity="left"
            android:orientation="horizontal"
            android:id="@+id/restricted_tip_layout"
            android:background="@drawable/bg_border_ffebef_corner_notice"
            android:paddingLeft="@dimen/margin_13dp"
            android:paddingTop="@dimen/margin_15dp"
            android:paddingRight="@dimen/margin_13dp"
            android:paddingBottom="@dimen/margin_15dp"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/margin_18dp">
            <ImageView
                android:id="@+id/ticklet_perform_item_status_icon"
                android:layout_width="12dp"
                android:layout_height="12dp"
                android:layout_marginTop="@dimen/margin_3dp"
                android:layout_marginRight="@dimen/margin_4dp"
                android:src="@drawable/ic_prohibit_red"/>
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="13dp"
                    android:textStyle="bold"
                    android:textColor="#ff335c"
                    android:id="@+id/restricted_tip_content_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:maxLines="1"/>
                <TextView
                    android:textSize="12dp"
                    android:textColor="#ff335c"
                    android:id="@+id/restricted_tip_content"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_9dp"/>
            </LinearLayout>
        </LinearLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textSize="@dimen/resource_size_16"
                android:textColor="@color/black"
                android:ellipsize="end"
                android:id="@+id/title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:maxLines="1"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/resource_size_10"
                android:layout_alignParentStart="true"/>
            <ImageView
                android:id="@+id/title_icon"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:scaleType="fitXY"
                android:layout_centerVertical="true"
                android:layout_toEndOf="@+id/title"/>
            <cn.damai.uikit.iconfont.DMIconFontTextView
                android:textSize="@dimen/resource_size_13"
                android:textColor="@color/color_999999"
                android:id="@+id/right_arrow"
                android:paddingTop="@dimen/margin_1dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/iconfont_youjiantou24"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true"/>
        </RelativeLayout>
        <TextView
            android:textSize="@dimen/resource_size_13"
            android:textColor="@color/color_999999"
            android:ellipsize="end"
            android:id="@+id/content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/resource_size_8"
            android:maxLines="2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/title"/>
        <LinearLayout
            android:gravity="left"
            android:orientation="horizontal"
            android:id="@+id/service_tip_layout"
            android:background="@drawable/bg_border_ff993a_corner_notice"
            android:paddingLeft="@dimen/margin_7dp"
            android:paddingTop="@dimen/margin_6dp"
            android:paddingRight="@dimen/margin_7dp"
            android:paddingBottom="@dimen/margin_6dp"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_8dp">
            <cn.damai.uikit.iconfont.DMIconFontTextView
                android:textSize="16dp"
                android:textColor="#ff993a"
                android:id="@+id/service_tip_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin_10dp"
                android:layout_marginRight="@dimen/margin_4dp"
                android:text="@string/iconfont_tixing24"/>
            <TextView
                android:textSize="13dp"
                android:textColor="@color/color_666666"
                android:id="@+id/service_tip_content"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>
    </LinearLayout>
</FrameLayout>
