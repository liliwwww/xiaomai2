<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <cn.damai.ticklet.view.TickletTicketStandPortalViewNew
        android:gravity="center"
        android:layout_gravity="center"
        android:id="@+id/ticklet_ticket_stand_portal_view"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        app:view_type="DEFAULT"/>
    <RelativeLayout
        android:orientation="vertical"
        android:id="@+id/ticklet_ticket_tvip_icon_layout"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="-0.5dp"
        android:layout_below="@+id/ticklet_ticket_stand_portal_view"
        android:layout_alignParentLeft="true">
        <View
            android:id="@+id/ticklet_ticket_tvip_icon_top_bg"
            android:background="#ffe3f1"
            android:layout_width="match_parent"
            android:layout_height="@dimen/margin_10dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"/>
        <View
            android:id="@+id/ticklet_ticket_tvip_icon_top_bg_holder"
            android:background="@drawable/ticklet_ffe3f1_radius_2_bg"
            android:layout_width="match_parent"
            android:layout_height="@dimen/margin_6dp"
            android:layout_marginTop="@dimen/margin_6dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"/>
        <ImageView
            android:layout_gravity="center_horizontal"
            android:id="@+id/ticklet_ticket_ll_tm_desc"
            android:layout_width="150dp"
            android:layout_height="@dimen/margin_17dp"
            android:layout_marginTop="@dimen/margin_6dp"
            android:src="@drawable/ticket_vip_diamond"
            android:scaleType="fitXY"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"/>
    </RelativeLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:id="@+id/ticklet_ticket_num_tip_layout"
        android:paddingTop="@dimen/margin_10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ticklet_ticket_stand_portal_view"
        android:layout_alignParentLeft="true">
        <TextView
            android:textSize="16dp"
            android:textColor="@color/color_FF2D79"
            android:layout_gravity="center_vertical"
            android:id="@+id/bnt_position"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_12dp"
            android:lineSpacingExtra="0dp"/>
        <TextView
            android:layout_gravity="center_vertical"
            android:id="@+id/ticklet_ticket_head_num_line"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="/"
            style="@style/ticklet_000000_16dp"/>
        <TextView
            android:layout_gravity="center_vertical"
            android:id="@+id/tv_total"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/ticklet_000000_16dp"/>
        <TextView
            android:layout_gravity="bottom"
            android:id="@+id/tv_ticket_unit"
            android:paddingLeft="@dimen/margin_2dp"
            android:paddingBottom="2.5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="张"
            style="@style/ticklet_000000_10dp"/>
    </LinearLayout>
    <LinearLayout
        android:gravity="center"
        android:orientation="horizontal"
        android:id="@+id/ticklet_ticket_ll_forget_card"
        android:paddingLeft="@dimen/margin_4dp"
        android:paddingTop="@dimen/margin_10dp"
        android:paddingBottom="@dimen/margin_2dp"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="12dp"
        android:layout_below="@+id/ticklet_ticket_stand_portal_view"
        android:layout_alignParentRight="true">
        <TextView
            android:layout_gravity="center_vertical"
            android:id="@+id/ticklt_forget_card_tip_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="忘带证件"
            android:layout_marginStart="@dimen/margin_2dp"
            style="@style/ticklet_ff2d79_13dp"/>
    </LinearLayout>
    <TextView
        android:textSize="@dimen/resource_size_13"
        android:textColor="#0091ff"
        android:id="@+id/ticklet_ticket_ll_trip_more_info"
        android:paddingLeft="@dimen/resource_size_12"
        android:paddingTop="@dimen/resource_size_10"
        android:paddingRight="@dimen/resource_size_12"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="更多使用提示"
        android:layout_below="@+id/ticklet_ticket_stand_portal_view"
        android:layout_alignParentRight="true"/>
</merge>
