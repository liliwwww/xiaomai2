<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/ticklet_head_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <include
        android:id="@+id/ticket_list_rl_emergency_notification"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/margin_12dp"
        android:layout_marginRight="@dimen/margin_12dp"
        android:layout_marginBottom="9dp"
        android:layout_alignParentTop="true"
        layout="@layout/ticklet_list_header_notice"/>
    <include
        android:id="@+id/ticklet_list_calendar_vip_entrance_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:layout_below="@+id/ticket_list_rl_emergency_notification"
        layout="@layout/ticklet_list_calendar_vip_layout"/>
    <include
        android:id="@+id/ticklet_list_transfer_message_layout"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/margin_12dp"
        android:layout_below="@+id/ticklet_list_calendar_vip_entrance_layout"
        layout="@layout/ticklet_list_transfer_title"/>
    <LinearLayout
        android:gravity="center_horizontal"
        android:orientation="vertical"
        android:id="@+id/ticklet_perform_banner_login_shadow"
        android:paddingLeft="@dimen/margin_15dp"
        android:paddingTop="@dimen/margin_10dp"
        android:paddingRight="@dimen/margin_15dp"
        android:paddingBottom="@dimen/margin_20dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_4dp"
        android:layout_below="@+id/ticklet_list_transfer_message_layout">
        <LinearLayout
            android:gravity="center_horizontal"
            android:orientation="vertical"
            android:id="@+id/ticklet_perform_banner_login"
            android:background="@drawable/ticklet_list_nologin_bg"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <cn.damai.uikit.banner.Banner
                android:id="@+id/ticklet_banner"
                android:layout_width="wrap_content"
                android:layout_height="124dp"
                android:layout_marginLeft="@dimen/margin_18dp"
                android:layout_marginTop="64dp"
                android:layout_marginRight="@dimen/margin_18dp"
                app:image_scale_type="fit_center"
                app:indicator_drawable_selected="@drawable/ticklet_viewpager_circleindicator_selected"
                app:indicator_drawable_unselected="@drawable/ticklet_viewpager_circleindicator_unselected"
                app:indicator_height="7dp"
                app:indicator_margin="3dp"
                app:indicator_width="7dp"/>
            <TextView
                android:gravity="center"
                android:id="@+id/ticklet_nologin_main_tips"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_15dp"
                android:text="@string/ticklet_list_nologin_perform_title_tips"
                style="@style/ticklet_000000_16dp"/>
            <TextView
                android:gravity="center"
                android:id="@+id/ticklet_nologin_sub_tips"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:text="@string/ticklet_list_nologin_perform_subtitle_tips"
                style="@style/ticklet_999999_13dp"/>
            <LinearLayout
                android:gravity="center"
                android:orientation="horizontal"
                android:id="@+id/indicator_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin_18dp"
                android:layout_marginTop="@dimen/margin_12dp"
                android:layout_marginRight="@dimen/margin_18dp"/>
            <cn.damai.uikit.view.DMActionButtonBgView
                android:gravity="center"
                android:layout_gravity="center"
                android:id="@+id/ticklet_list_nologin_btn_text"
                android:layout_width="111dp"
                android:layout_height="40dp"
                android:layout_marginTop="@dimen/margin_30dp"
                android:layout_marginBottom="36dp"
                android:layout_centerHorizontal="true"/>
        </LinearLayout>
        <ImageView
            android:id="@+id/ticklet_list_top_bg"
            android:background="@drawable/ticklet_list_nologin_top"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</merge>
