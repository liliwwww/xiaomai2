<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:textSize="14dp"
        android:textColor="@color/color_000000"
        android:id="@+id/aliuser_protocal_tip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_9dp"
        android:layout_marginBottom="4.5dp"
        android:text="请您认真阅读并同意《大麦会员服务协议》《订票服务条款》《隐私权政策》的全部条款。"
        android:lineSpacingExtra="4.5dp"/>
    <ScrollView
        android:id="@+id/dialog_content_container"
        android:scrollbars="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:overScrollMode="ifContentScrolls">
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/aliuser_damai_ll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_gravity="right"
                android:id="@+id/aliuser_protocol_cancel"
                android:visibility="gone"
                android:clickable="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ali_ucc_dialog_close"/>
            <TextView
                android:textAppearance="@style/AliUserMenuTextAppearance"
                android:textColor="@color/aliuser_taobaox_protocol_head_blue"
                android:gravity="center_horizontal"
                android:id="@+id/aliuser_reg_tip"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/aliuser_space_12"
                android:layout_marginBottom="@dimen/aliuser_space_12"/>
            <TextView
                android:textSize="13dp"
                android:textColor="@color/color_333333"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="亲，您好！同意下述协议和条款才能登录大麦APP哦。             为了让您更好了解我们对您的权益保护，特地向使用大麦网服务的您推送本提示。"
                android:lineSpacingExtra="4.5dp"/>
            <TextView
                android:textSize="13dp"
                android:textColor="@color/color_666666"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/aliuser_damai_protocol_explain"
                android:lineSpacingExtra="4.5dp"/>
            <TextView
                android:textSize="13dp"
                android:textColor="@color/color_666666"
                android:id="@+id/aliuser_protocal_tip"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_9dp"
                android:text="您可以阅读我们完整的《大麦用户服务协议》、《隐私权政策》、《订票服务条款》了解我们的承诺。"
                android:lineSpacingExtra="4.5dp"/>
            <TextView
                android:textSize="14dp"
                android:textColor="@color/aliuser_color_orange"
                android:id="@+id/aliuser_tb_protocal"
                android:visibility="gone"
                android:layout_marginBottom="@dimen/aliuser_space_8"
                android:text="@string/aliuser_damai_protocal"
                style="@style/AliUserFuncTextAppearance"/>
            <TextView
                android:textSize="14dp"
                android:textColor="@color/aliuser_color_orange"
                android:id="@+id/aliuser_tb_protocal"
                android:visibility="gone"
                android:layout_marginBottom="@dimen/aliuser_space_8"
                android:text="@string/aliuser_damai_protocal"
                style="@style/AliUserFuncTextAppearance"/>
            <TextView
                android:textSize="14dp"
                android:textColor="@color/aliuser_color_orange"
                android:id="@+id/aliuser_policy_protocal"
                android:visibility="gone"
                android:layout_marginBottom="@dimen/aliuser_space_8"
                android:text="@string/aliuser_damai_policy_protocal"
                style="@style/AliUserFuncTextAppearance"/>
            <TextView
                android:textSize="14dp"
                android:textColor="@color/aliuser_color_orange"
                android:id="@+id/aliuser_alipay_protocal"
                android:visibility="gone"
                android:layout_marginBottom="@dimen/aliuser_space_8"
                android:text="@string/aliuser_alipay_protocal"
                style="@style/AliUserFuncTextAppearance"/>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
