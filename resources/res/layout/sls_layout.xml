<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <Button
        android:id="@+id/upload"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="上传log数据"
        android:layout_alignParentTop="true"/>
    <TextView
        android:id="@+id/count_tip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_30dp"
        android:text="上传次数"
        android:layout_below="@+id/upload"/>
    <EditText
        android:id="@+id/et_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_18dp"
        android:text="10"
        android:layout_toRightOf="@+id/count_tip"
        android:layout_below="@+id/upload"/>
    <TextView
        android:id="@+id/count_errorcode_tip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="45dp"
        android:text="上传code"
        android:layout_below="@+id/count_tip"/>
    <EditText
        android:id="@+id/et_errorcode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_18dp"
        android:text="-5500"
        android:layout_toRightOf="@+id/count_errorcode_tip"
        android:layout_below="@+id/et_count"/>
</RelativeLayout>
