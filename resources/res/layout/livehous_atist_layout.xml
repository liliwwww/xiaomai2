<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@color/white"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/margin_12dp">
    <FrameLayout
        android:paddingLeft="21dp"
        android:paddingRight="21dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <include layout="@layout/online_card_title_layout"/>
    </FrameLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_14dp">
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/livehouse_artist_recycler"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </FrameLayout>
    <cn.damai.baseview.RCFrameLayoutLayoutView
        android:layout_width="match_parent"
        android:layout_height="188dp"
        android:layout_marginLeft="@dimen/margin_21dp"
        android:layout_marginTop="@dimen/margin_14dp"
        android:layout_marginRight="@dimen/margin_21dp"
        app:round_corner="12dp"
        app:stroke_color="#1a000000"
        app:stroke_width="0.5dp">
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/livehouse_artist_content_recycler"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </cn.damai.baseview.RCFrameLayoutLayoutView>
</LinearLayout>
