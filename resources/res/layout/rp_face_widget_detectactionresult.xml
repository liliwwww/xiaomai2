<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.alibaba.security.realidentity.ui.widgets.DetectActionResultWidget
        android:id="@+id/widget_abfl_detectactionresult"
        android:background="@color/rpsdk_white"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:gravity="center"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerInParent="true">
            <ImageView
                android:id="@+id/abfl_widget_dar_icon"
                android:visibility="visible"
                android:layout_width="165px"
                android:layout_height="165px"
                android:scaleType="fitXY"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"/>
            <TextView
                android:textSize="42px"
                android:textColor="#333333"
                android:gravity="center_horizontal"
                android:id="@+id/abfl_widget_dar_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="60px"
                android:text="@string/face_liveness_recognize_fail"
                android:layout_centerHorizontal="true"/>
            <TextView
                android:textSize="30px"
                android:textColor="#333333"
                android:gravity="center"
                android:id="@+id/abfl_widget_dar_content"
                android:visibility="invisible"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30px"
                android:text="Message2"
                android:layout_centerHorizontal="true"/>
            <Button
                android:textSize="32px"
                android:textColor="@android:color/white"
                android:gravity="center"
                android:id="@+id/abfl_widget_dar_btn"
                android:background="@drawable/rp_face_nav_button_back"
                android:paddingLeft="0dp"
                android:paddingTop="0dp"
                android:paddingRight="0dp"
                android:paddingBottom="0dp"
                android:layout_width="match_parent"
                android:layout_height="88px"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="50px"
                android:layout_marginRight="15dp"/>
            <Button
                android:textSize="32px"
                android:textColor="@android:color/black"
                android:gravity="center"
                android:id="@+id/abfl_widget_dar_ctid_btn"
                android:background="@drawable/rp_face_nav_button_retry"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="88px"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="35px"
                android:layout_marginRight="15dp"
                android:text="@string/rp_ctid_verify_retry"/>
            <Button
                android:textSize="28px"
                android:textColor="#3898e1"
                android:gravity="center"
                android:id="@+id/abfl_widget_dar_other_btn"
                android:background="#00000000"
                android:layout_width="match_parent"
                android:layout_height="96px"
                android:layout_marginLeft="30px"
                android:layout_marginTop="35px"
                android:layout_marginRight="30px"
                android:text="@string/face_detect_result_login"
                android:layout_centerHorizontal="true"/>
        </LinearLayout>
    </com.alibaba.security.realidentity.ui.widgets.DetectActionResultWidget>
</merge>
