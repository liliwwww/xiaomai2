<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:paddingTop="@dimen/margin_12dp"
    android:paddingBottom="@dimen/margin_12dp"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <cn.damai.uikit.iconfont.DMIconFontTextView
        android:textSize="@dimen/margin_13dp"
        android:textColor="@color/color_FF1268"
        android:id="@+id/project_support_indicator_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/iconfont_dagou12"/>
    <TextView
        android:textSize="@dimen/margin_13dp"
        android:textColor="@color/color_111111"
        android:id="@+id/project_support_content_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/margin_3dp"
        android:layout_marginRight="@dimen/margin_15dp"/>
</LinearLayout>
