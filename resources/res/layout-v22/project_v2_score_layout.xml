<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:ratingbar="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/bg_border_corner_white_9"
    android:visibility="gone"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.alibaba.pictures.moimage.MoImageView
        android:id="@+id/project_score_bg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scaleType="fitXY"
        ratingbar:roundBottomLeft="true"
        ratingbar:roundBottomRight="true"
        ratingbar:roundTopLeft="true"
        ratingbar:roundTopRight="true"
        ratingbar:roundedCornerRadius="8dp"/>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:id="@+id/layout_score_tip"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:layout_width="103dp"
                    android:layout_height="36dp"
                    android:src="?attr/bgDetailScoreTitle"/>
                <cn.damai.uikit.iconfont.DMIconFontTextView
                    android:textSize="14dp"
                    android:textColor="@color/bricks_9c9ca5"
                    android:id="@+id/tv_score_icon_tip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/iconfont_tixingxian_"
                    android:paddingStart="7dp"
                    android:paddingEnd="5dp"/>
                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"/>
                <cn.damai.uikit.iconfont.DMIconFontTextView
                    android:textSize="10dp"
                    android:textColor="#959aa5"
                    android:id="@+id/tv_score_to_comment"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="9dp"
                    android:text="@string/iconfont_youjiantou12"/>
            </LinearLayout>
            <FrameLayout
                android:id="@+id/layout_score_content"
                android:paddingLeft="12dp"
                android:paddingRight="20dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <LinearLayout
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp">
                    <RelativeLayout
                        android:gravity="bottom"
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="57dp">
                        <RelativeLayout
                            android:gravity="center_vertical"
                            android:orientation="horizontal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="0dp">
                            <cn.damai.uikit.number.DMDigitTextView
                                android:textSize="28dp"
                                android:textColor="?attr/bgDetailLikeTextSelect"
                                android:id="@+id/tv_score"
                                android:visibility="gone"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                            <com.alibaba.pictures.bricks.view.FakeBoldTextView
                                android:textSize="12dp"
                                android:textColor="#2e333e"
                                android:id="@+id/tv_socre_desc"
                                android:visibility="gone"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="4dp"
                                android:layout_marginTop="14.5dp"
                                android:layout_toRightOf="@+id/tv_score"/>
                        </RelativeLayout>
                        <LinearLayout
                            android:gravity="center_vertical"
                            android:orientation="horizontal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="0dp"
                            android:layout_marginBottom="1px"
                            android:layout_alignParentBottom="true">
                            <cn.damai.uikit.view.DMRatingBar
                                android:id="@+id/prj_score_star_view"
                                android:visibility="gone"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="6dp"
                                ratingbar:isIndicator="true"
                                ratingbar:showText="false"
                                ratingbar:starCount="5"
                                ratingbar:starDistance="@dimen/margin_1dp"
                                ratingbar:starEmpty="@drawable/rating_empty"
                                ratingbar:starFill="?attr/bgRatingBarStar"
                                ratingbar:starSize="9dp"/>
                            <TextView
                                android:textSize="10dp"
                                android:textColor="#959aa5"
                                android:id="@+id/tv_rating_num_desc"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="4dp"/>
                        </LinearLayout>
                    </RelativeLayout>
                    <View
                        android:id="@+id/header_score_devide_line"
                        android:background="#1aff901c"
                        android:layout_width="0.5dp"
                        android:layout_height="52dp"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="8dp"/>
                    <LinearLayout
                        android:layout_gravity="bottom"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <ViewFlipper
                            android:gravity="center_vertical"
                            android:id="@+id/header_project_right_view_flipper"
                            android:layout_width="wrap_content"
                            android:layout_height="57dp"
                            android:inAnimation="@anim/flipper_in_alpha"
                            android:outAnimation="@anim/flipper_out_alpha">
                            <include
                                android:id="@+id/header_service_etc_ui"
                                layout="@layout/project_v2_score_right_item"/>
                        </ViewFlipper>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
            <LinearLayout
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:id="@+id/want_see_rank_list_ll"
                android:paddingLeft="12dp"
                android:paddingRight="12dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="9dp"
                android:paddingHorizontal="12dp">
                <com.alibaba.pictures.moimage.MoImageView
                    android:background="#26959aa5"
                    android:layout_width="match_parent"
                    android:layout_height="1px"/>
                <LinearLayout
                    android:gravity="center_vertical"
                    android:id="@+id/score_bottom_wanna_see_ui"
                    android:layout_width="match_parent"
                    android:layout_height="42dp">
                    <LinearLayout
                        android:id="@+id/want_see_number_ll"
                        android:visibility="gone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <cn.damai.uikit.number.DMDigitTextView
                            android:textSize="14dp"
                            android:textColor="#2e333e"
                            android:id="@+id/want_see_number_dtv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                        <TextView
                            android:textSize="12dp"
                            android:textColor="#2e333e"
                            android:id="@+id/want_see_number_suffix_dtv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>
                    <LinearLayout
                        android:gravity="center_vertical"
                        android:id="@+id/want_see_rank_ll"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <View
                            android:id="@+id/want_see_rank_space_before"
                            android:visibility="gone"
                            android:layout_width="0dp"
                            android:layout_height="1dp"
                            android:layout_weight="1"/>
                        <com.alibaba.pictures.moimage.MoImageView
                            android:id="@+id/want_see_rank_pic"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:src="?attr/bgDetailScoreBangLogo"
                            android:scaleType="fitXY"/>
                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                            <TextView
                                android:textSize="12dp"
                                android:textColor="#2e333e"
                                android:id="@+id/want_see_rank_list_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="4dp"
                                android:text="话剧人气榜"
                                android:includeFontPadding="false"/>
                            <cn.damai.uikit.number.DMDigitTextView
                                android:textSize="12dp"
                                android:textColor="#2e333e"
                                android:id="@+id/want_see_rank_num"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_toRightOf="@+id/want_see_rank_list_text"
                                android:layout_alignBaseline="@+id/want_see_rank_list_text"/>
                        </RelativeLayout>
                        <View
                            android:id="@+id/want_see_rank_space_after"
                            android:layout_width="0dp"
                            android:layout_height="1dp"
                            android:layout_weight="1"/>
                        <cn.damai.uikit.iconfont.DMIconFontTextView
                            android:textSize="10dp"
                            android:textColor="#959aa5"
                            android:id="@+id/want_see_rank_list_iconf"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="6dp"
                            android:text="@string/iconfont_youjiantou12"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</FrameLayout>
