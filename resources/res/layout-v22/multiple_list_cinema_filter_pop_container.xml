<?xml version="1.0" encoding="utf-8"?>
<com.taobao.movie.android.app.order.ui.widget.TrackerFrameLayout2 xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.google.android.flexbox.FlexboxLayout
        android:orientation="vertical"
        android:background="#99000000"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:flexDirection="column"
        app:flexWrap="nowrap">
        <LinearLayout
            android:id="@+id/current_city_container"
            android:background="@color/white"
            android:paddingLeft="12dp"
            android:paddingRight="12dp"
            android:paddingBottom="12dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingHorizontal="12dp"
            app:layout_flexShrink="0">
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:id="@+id/current_city_ll"
                android:background="@drawable/multiple_list_cinema_filter_pop_location_container_bg"
                android:paddingLeft="9dp"
                android:paddingRight="9dp"
                android:layout_width="match_parent"
                android:layout_height="27dp"
                android:paddingHorizontal="9dp">
                <com.taobao.movie.android.commonui.widget.IconFontTextView
                    android:textSize="14dp"
                    android:textColor="#148aff"
                    android:id="@+id/iconf_current_city"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="4dp"
                    android:text="@string/icon_font_ticket_location"/>
                <TextView
                    android:textSize="13dp"
                    android:textColor="#2e333e"
                    android:id="@+id/current_city_tag"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="当前城市："/>
                <TextView
                    android:textSize="12dp"
                    android:textColor="#2e333e"
                    android:id="@+id/current_city_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <View
                    android:layout_width="0dp"
                    android:layout_height="1px"
                    android:layout_weight="1"/>
                <TextView
                    android:textSize="13dp"
                    android:textColor="#2e333e"
                    android:id="@+id/current_city_exchange"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="切换城市"/>
                <com.taobao.movie.android.commonui.widget.IconFontTextView
                    android:textSize="9dp"
                    android:textColor="#2e333e"
                    android:id="@+id/current_city_exchange_iconf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/icon_font_arrow_right"/>
            </LinearLayout>
        </LinearLayout>
        <View
            android:id="@+id/type_divider"
            android:background="@color/common_text_color14"
            android:layout_width="match_parent"
            android:layout_height="1px"
            app:layout_flexShrink="0"/>
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/type_container"
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            app:layout_flexShrink="0"/>
        <View
            android:background="@color/common_text_color14"
            android:layout_width="match_parent"
            android:layout_height="1px"
            app:layout_flexShrink="0"/>
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <View
                android:layout_gravity="bottom"
                android:id="@+id/fill_bg_layout"
                android:background="#99000000"
                android:clickable="false"
                android:layout_width="match_parent"
                android:layout_height="6dp"/>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_flexShrink="1">
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/group_list"
                    android:background="@drawable/filter_pop_container_bl_bg"
                    android:paddingBottom="12dp"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/filter_list"
                    android:background="@drawable/filter_pop_container_br_bg"
                    android:paddingBottom="12dp"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"/>
            </LinearLayout>
        </FrameLayout>
        <View
            android:id="@+id/flex_bg_layout"
            android:background="#99000000"
            android:clickable="false"
            android:layout_width="match_parent"
            android:layout_height="275dp"
            android:minHeight="275dp"
            android:layout_weight="1"
            app:layout_flexGrow="1"
            app:layout_flexShrink="1"/>
    </com.google.android.flexbox.FlexboxLayout>
</com.taobao.movie.android.app.order.ui.widget.TrackerFrameLayout2>
