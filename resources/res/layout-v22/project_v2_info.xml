<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/project_info_container_ll"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <cn.damai.trade.newtradeorder.ui.projectdetail.projectdetailnew.video.ProjectPicAndVideoView
        android:gravity="center_horizontal"
        android:id="@+id/layout_video_and_pic"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="247dp"/>
    <FrameLayout
        android:id="@+id/project_info_container_fl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.alibaba.pictures.moimage.MoImageView
            android:id="@+id/project_info_container_bg_default"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="213dp"
            android:scaleType="matrix"/>
        <ImageView
            android:id="@+id/project_info_container_bg"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <ImageView
            android:background="@drawable/bg_project_v2_info_black_gradient"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/project_info_container_info"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="46dp">
            <LinearLayout
                android:id="@+id/project_info_notice_flipper_ll"
                android:background="@drawable/bg_border_project_info_notice"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="9dp"
                android:layout_marginRight="9dp"
                android:layout_marginBottom="9dp"
                android:layout_marginHorizontal="9dp">
                <ViewFlipper
                    android:id="@+id/project_info_notice_flipper"
                    android:layout_width="match_parent"
                    android:layout_height="33dp"
                    android:inAnimation="@anim/flipper_in"
                    android:outAnimation="@anim/flipper_out">
                    <include
                        android:id="@+id/info_notice_ui"
                        layout="@layout/project_v2_info_notice"/>
                </ViewFlipper>
            </LinearLayout>
            <LinearLayout
                android:layout_gravity="bottom"
                android:orientation="horizontal"
                android:id="@+id/project_info_container"
                android:paddingLeft="12dp"
                android:paddingTop="12dp"
                android:paddingRight="12dp"
                android:paddingBottom="22dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="9dp"
                android:layout_marginRight="9dp"
                android:layout_marginHorizontal="9dp"
                android:paddingHorizontal="12dp">
                <FrameLayout
                    android:id="@+id/project_poster_fl"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">
                    <cn.damai.uikit.view.DMPosterView
                        android:id="@+id/project_poster"
                        android:layout_width="78dp"
                        android:layout_height="104dp"
                        android:layout_marginRight="12dp"
                        android:src="@drawable/uikit_default_image_bg_gradient"/>
                    <com.alibaba.pictures.bricks.view.FakeBoldTextView
                        android:textSize="9dp"
                        android:textColor="@color/white"
                        android:gravity="center"
                        android:id="@+id/project_poster_tv"
                        android:paddingLeft="3dp"
                        android:paddingRight="3dp"
                        android:visibility="gone"
                        android:layout_width="wrap_content"
                        android:layout_height="16dp"
                        android:layout_marginLeft="3dp"
                        android:layout_marginTop="3dp"
                        android:paddingHorizontal="3dp"/>
                    <com.alibaba.pictures.bricks.view.PuHuiTiTextView
                        android:textSize="9dp"
                        android:textColor="@color/white"
                        android:gravity="center"
                        android:id="@+id/project_poster_tv_pioneer"
                        android:paddingLeft="3dp"
                        android:paddingRight="3dp"
                        android:visibility="gone"
                        android:layout_width="wrap_content"
                        android:layout_height="16dp"
                        android:layout_marginLeft="3dp"
                        android:layout_marginTop="3dp"
                        android:paddingHorizontal="3dp"/>
                </FrameLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="104dp">
                    <LinearLayout
                        android:orientation="vertical"
                        android:id="@+id/project_title_ll"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <FrameLayout
                            android:id="@+id/project_title_layout1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <LinearLayout
                                android:orientation="horizontal"
                                android:id="@+id/project_show_tags_fl"
                                android:visibility="invisible"
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:layout_marginBottom="@dimen/margin_6dp"/>
                            <com.alibaba.pictures.bricks.view.FakeBoldTextView
                                android:textSize="16dp"
                                android:textColor="@color/C_white"
                                android:id="@+id/project_title_tv1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:maxLines="2"
                                android:includeFontPadding="false"/>
                        </FrameLayout>
                        <LinearLayout
                            android:gravity="center_vertical"
                            android:orientation="horizontal"
                            android:id="@+id/project_title_layout2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <com.alibaba.pictures.bricks.view.FakeBoldTextView
                                android:textSize="16dp"
                                android:textColor="@color/C_white"
                                android:ellipsize="end"
                                android:id="@+id/project_title_tv2"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:maxLines="1"
                                android:includeFontPadding="false"
                                android:layout_weight="1"/>
                            <cn.damai.uikit.iconfont.DMIconFontTextView
                                android:textSize="10dp"
                                android:textColor="@color/C_white"
                                android:gravity="center"
                                android:id="@+id/project_title_iconfont"
                                android:layout_width="18dp"
                                android:layout_height="18dp"
                                android:text="@string/iconfont_Gengduo18"/>
                        </LinearLayout>
                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"/>
                    <LinearLayout
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:id="@+id/project_time_ll"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp">
                        <cn.damai.uikit.number.DMDigitTextView
                            android:textSize="16dp"
                            android:textColor="#e6ffffff"
                            android:ellipsize="end"
                            android:id="@+id/project_timer_day_star"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:maxLines="1"/>
                        <View
                            android:id="@+id/project_timer_day_div"
                            android:background="@color/white"
                            android:layout_width="10dp"
                            android:layout_height="1.5dp"
                            android:layout_marginLeft="1.5dp"
                            android:layout_marginRight="1.5dp"
                            android:layout_marginHorizontal="1.5dp"/>
                        <cn.damai.uikit.number.DMDigitTextView
                            android:textSize="16dp"
                            android:textColor="#e6ffffff"
                            android:ellipsize="end"
                            android:id="@+id/project_timer_day_end"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:maxLines="1"/>
                    </LinearLayout>
                    <TextView
                        android:textSize="12dp"
                        android:textColor="#a6ffffff"
                        android:ellipsize="end"
                        android:id="@+id/project_time_tips_info"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="1dp"
                        android:maxLines="2"
                        android:includeFontPadding="false"
                        android:lineSpacingMultiplier="0.9"
                        app:layout_constraintBottom_toBottomOf="@+id/project_poster_fl"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/project_poster_fl"
                        app:layout_constraintTop_toBottomOf="@+id/project_time_ll"
                        app:layout_constraintVertical_bias="1"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</LinearLayout>
