<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="14dp"/>
            <gradient
                android:startColor="#fbdb0e"
                android:endColor="#ffb32b"
                android:angle="cw_0"
                android:type="linear"
                android:centerColor="#fb630e"/>
        </shape>
    </item>
    <item
        android:left="1dp"
        android:top="1dp"
        android:right="1dp"
        android:bottom="1dp">
        <shape android:shape="rectangle">
            <corners android:radius="14dp"/>
            <solid android:color="@color/white"/>
        </shape>
    </item>
    <item
        android:left="1dp"
        android:top="1dp"
        android:right="1dp"
        android:bottom="1dp">
        <shape android:shape="rectangle">
            <corners android:radius="14dp"/>
            <gradient
                android:startColor="#14f5ac6b"
                android:endColor="#14f7846d"
                android:angle="cw_0"
                android:type="linear"/>
        </shape>
    </item>
</layer-list>
