<?xml version="1.0" encoding="utf-8"?>
<DFrameLayout xmlns:dinamic="http://schemas.android.com/apk/res-auto"
    dinamic:dWidth="375ap"
    dinamic:dHeight="match_content"
    dinamic:dBackgroundColor="#ffffff"
    dinamic:dAccessibilityTextHidden="false">
    <DLinearLayout
        dinamic:dMarginLeft="12np"
        dinamic:dMarginRight="12np"
        dinamic:dMarginTop="7np"
        dinamic:dMarginBottom="7np"
        dinamic:dWidth="match_parent"
        dinamic:dHeight="match_content">
        <DLinearLayout
            dinamic:dOrientation="vertical"
            dinamic:dMarginRight="10np"
            dinamic:dWidth="0np"
            dinamic:dHeight="match_content"
            dinamic:dGravity="leftCenter"
            dinamic:dWeight="1.0">
            <DLinearLayout
                dinamic:dOrientation="horizontal"
                dinamic:dWidth="match_parent"
                dinamic:dHeight="match_content"
                dinamic:dGravity="leftCenter">
                <DTextView
                    dinamic:dText="@data{fields.count}"
                    dinamic:dTextSize="13np"
                    dinamic:dTextColor="#999999"
                    dinamic:dTextAlignment="right"
                    dinamic:dMaxLines="1"
                    dinamic:dMarginRight="3np"
                    dinamic:dWidth="0np"
                    dinamic:dHeight="match_content"
                    dinamic:dAccessibilityTextHidden="false"
                    dinamic:dFocusable="false"
                    dinamic:dGravity="leftCenter"
                    dinamic:dWeight="1.0"/>
                <DTextView
                    dinamic:dText="@data{fields.priceTitle}"
                    dinamic:dTextSize="14np"
                    dinamic:dTextColor="#333333"
                    dinamic:dTextAlignment="right"
                    dinamic:dMaxLines="1"
                    dinamic:dWidth="match_content"
                    dinamic:dHeight="match_content"
                    dinamic:dAccessibilityTextHidden="false"
                    dinamic:dFocusable="false"
                    dinamic:dGravity="leftCenter"/>
                <TradePriceView
                    dinamic:dTextSize="18np"
                    dinamic:dTextColor="@theme{&apos;color&apos;, &apos;themeColor&apos;, &apos;#FF5000&apos;}"
                    dinamic:dTextAlignment="right"
                    dinamic:dMaxLines="1"
                    dinamic:dLineBreakMode="end"
                    dinamic:dWidth="match_content"
                    dinamic:dHeight="match_content"
                    dinamic:dAccessibilityTextHidden="false"
                    dinamic:dFocusable="false"
                    dinamic:dGravity="leftCenter"
                    dinamic:dPrice="@data{fields.price}"
                    dinamic:dSymbolScale="0.73"
                    dinamic:dDecimalScale="0.73"/>
            </DLinearLayout>
            <DTextView
                dinamic:dText="@data{fields.desc}"
                dinamic:dTextSize="12np"
                dinamic:dTextColor="@data{fields.descCss.color}; @const{&apos;#99999&apos;}"
                dinamic:dMaxLines="2"
                dinamic:dLineBreakMode="end"
                dinamic:dMarginTop="1.3np"
                dinamic:dWidth="match_parent"
                dinamic:dHeight="match_content"
                dinamic:dVisibility="@triple{@data{fields.desc}, &apos;visible&apos;, &apos;gone&apos;}"
                dinamic:dAccessibilityTextHidden="false"
                dinamic:dFocusable="false"
                dinamic:dTextGravity="right"/>
        </DLinearLayout>
        <DFrameLayout
            dinamic:dWidth="100np"
            dinamic:dHeight="40np"
            dinamic:dVisibility="@triple{@data{fields.isShowFamilyPayBtn},&apos;visible&apos;,&apos;gone&apos;}"
            dinamic:dAccessibilityTextHidden="false"
            dinamic:dClipTopLeftRadius="20np"
            dinamic:dClipBottomLeftRadius="20np">
            <DImageView
                dinamic:dScaleType="centerCrop"
                dinamic:dWidth="match_parent"
                dinamic:dHeight="match_parent"
                dinamic:dGravity="center"
                dinamic:dImage="@theme{&apos;gradient&apos;, &apos;placeholder&apos;, &apos;#FFCA09|#FF9502&apos;}"/>
            <DTextView
                dinamic:dText="@data{fields.payBtn.familyBtnTitle}"
                dinamic:dTextSize="14np"
                dinamic:dTextColor="@triple{@eq{@data{status}, &apos;disable&apos;},&apos;#7FFFFFFF&apos;,&apos;#FFFFFF&apos;}"
                dinamic:dTextAlignment="center"
                dinamic:dWidth="match_parent"
                dinamic:dHeight="match_parent"
                dinamic:dAccessibilityText="@data{fields.payBtn.familyBtnTitle}"
                dinamic:dAccessibilityTextHidden="false"
                dinamic:dFocusable="false"
                dinamic:onTap="@handleDinamicXEvent{@const{leftSubmitBtnClick}}"
                dinamic:dTextGravity="center"/>
        </DFrameLayout>
        <DFrameLayout
            dinamic:dWidth="100np"
            dinamic:dHeight="40np"
            dinamic:dAccessibilityTextHidden="false"
            dinamic:dClipTopLeftRadius="@triple{@data{fields.isShowFamilyPayBtn},&apos;0np&apos;,&apos;20np&apos;}"
            dinamic:dClipBottomLeftRadius="@triple{@data{fields.isShowFamilyPayBtn},&apos;0np&apos;,&apos;20np&apos;}"
            dinamic:dClipTopRightRadius="20np"
            dinamic:dClipBottomRightRadius="20np">
            <DImageView
                dinamic:dScaleType="centerCrop"
                dinamic:dWidth="match_parent"
                dinamic:dHeight="match_parent"
                dinamic:dGravity="center"
                dinamic:dImage="@theme{&apos;gradient&apos;, &apos;gradientThemeColor&apos;, &apos;#FF8000|#FF5000&apos;}"/>
            <DTextView
                dinamic:dText="@data{fields.payBtn.title}"
                dinamic:dTextSize="14np"
                dinamic:dTextColor="@triple{@eq{@data{status}, &apos;disable&apos;},&apos;#7FFFFFFF&apos;,&apos;#FFFFFF&apos;}"
                dinamic:dTextAlignment="center"
                dinamic:dWidth="match_parent"
                dinamic:dHeight="match_parent"
                dinamic:dAccessibilityText="@data{fields.payBtn.title}"
                dinamic:dAccessibilityTextHidden="false"
                dinamic:dFocusable="false"
                dinamic:onTap="@handleDinamicXEvent{@const{itemClick}}"
                dinamic:dTextGravity="center"/>
        </DFrameLayout>
    </DLinearLayout>
</DFrameLayout>
