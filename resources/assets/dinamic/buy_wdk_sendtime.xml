<?xml version="1.0" encoding="utf-8"?>
<DFrameLayout xmlns:dx="http://schemas.android.com/apk/res-auto"
    dx:dWidth="375ap"
    dx:dHeight="44ap"
    dx:dAccessibilityTextHidden="false"
    dx:onTap="@handleDinamicXEvent{@const{itemClick}}"
    dx:dAccessibility="on"
    dx:accessibilityText="选择配送时间">
    <DFrameLayout
        dx:dMarginLeft="12np"
        dx:dMarginRight="12np"
        dx:dWidth="match_parent"
        dx:dHeight="match_parent"
        dx:dBackgroundColor="#ffffff"
        dx:dAccessibilityTextHidden="false"
        dx:onTap="@handleDinamicXEvent{@const{itemClick}}"
        dx:dAccessibility="on"
        dx:accessibilityText="选择配送时间">
        <DTextView
            dx:dText="@data{fields.title}"
            dx:dTextSize="14np"
            dx:dTextColor="#333333"
            dx:dMarginLeft="12np"
            dx:dWidth="match_content"
            dx:dHeight="44np"
            dx:dAccessibility="on"
            dx:dTextGravity="left|center"/>
        <DLinearLayout
            dx:dMarginRight="95np"
            dx:dWidth="match_content"
            dx:dHeight="44np"
            dx:dGravity="rightCenter">
            <DTextView
                dx:dText="@concat{@data{fields.selectedTime}, @const{&apos; &apos;}, @data{fields.postFeeDesc}}"
                dx:dTextSize="12np"
                dx:dTextColor="@theme{&apos;color&apos;, &apos;themeColor&apos;, &apos;#FFFF5000&apos;}"
                dx:dMarginRight="7np"
                dx:dWidth="match_content"
                dx:dHeight="44np"
                dx:dAccessibility="on"
                dx:dTextGravity="left|centor"
                dx:dIsBold="true"/>
            <DImageView
                dx:dImageUrl="@data{fields.serviceIconUrl}"
                dx:dMarginRight="7np"
                dx:dWidth="match_content"
                dx:dHeight="12np"
                dx:dVisibility="@triple{@data{fields.serviceIconUrl}, visible, gone}"
                dx:dGravity="center"/>
        </DLinearLayout>
        <DTextView
            dx:dText="@data{fields.optTitle}"
            dx:dTextSize="12np"
            dx:dTextColor="#333333"
            dx:dMarginRight="25np"
            dx:dWidth="match_content"
            dx:dHeight="44np"
            dx:dGravity="rightCenter"
            dx:dAccessibility="on"
            dx:dTextGravity="left|centor"/>
        <DFrameLayout
            dx:dMarginRight="10np"
            dx:dWidth="14np"
            dx:dHeight="14np"
            dx:dGravity="rightCenter">
            <DImageView
                dx:dScaleType="fitCenter"
                dx:dWidth="14np"
                dx:dHeight="14np"
                dx:dGravity="rightCenter"
                dx:dImageName="order_head_arrow"/>
        </DFrameLayout>
    </DFrameLayout>
</DFrameLayout>
