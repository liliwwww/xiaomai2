<?xml version="1.0" encoding="utf-8"?>
<DFrameLayout xmlns:dinamic="http://schemas.android.com/apk/res-auto"
    dinamic:dOrientation="horizontal"
    dinamic:dWidth="375ap"
    dinamic:dHeight="40np"
    dinamic:dAccessibilityTextHidden="false">
    <DFrameLayout
        dinamic:dOrientation="horizontal"
        dinamic:dMarginLeft="12np"
        dinamic:dMarginRight="12np"
        dinamic:dWidth="match_parent"
        dinamic:dHeight="40np"
        dinamic:dBackgroundColor="#ffffff"
        dinamic:dAccessibilityTextHidden="false">
        <DLinearLayout
            dinamic:dOrientation="horizontal"
            dinamic:dWidth="match_content"
            dinamic:dHeight="match_content"
            dinamic:dGravity="leftCenter">
            <DTextView
                dinamic:dText="@data{fields.title}"
                dinamic:dTextSize="13np"
                dinamic:dTextColor="#333333"
                dinamic:dTextAlignment="right"
                dinamic:dMaxLines="0"
                dinamic:dWidth="102np"
                dinamic:dHeight="match_content"
                dinamic:dAccessibilityTextHidden="false"
                dinamic:dFocusable="false"/>
        </DLinearLayout>
        <DLinearLayout
            dinamic:dOrientation="horizontal"
            dinamic:dMarginRight="15np"
            dinamic:dWidth="match_content"
            dinamic:dHeight="match_content"
            dinamic:dGravity="rightCenter">
            <DImageView
                dinamic:dScaleType="fitCenter"
                dinamic:dImageUrl="@triple{@ilt{@data{fields.min},@data{fields.quantity}},@const{&apos;https://img.alicdn.com/tfs/TB1FrOmtOMnBKNjSZFoXXbOSFXa-135-120.png&apos;},@const{&apos;https://img.alicdn.com/tfs/TB1s2HgtHZnBKNjSZFhXXc.oXXa-135-120.png&apos;}}"
                dinamic:dWidth="25np"
                dinamic:dHeight="25np"
                dinamic:dAccessibilityText="减少按钮"
                dinamic:dGravity="leftCenter"
                dinamic:onTap="@handleDinamicXEvent{@const{changeQuantity},@const{decrease}}"/>
            <DFrameLayout
                dinamic:dWidth="35np"
                dinamic:dHeight="40np">
                <DImageView
                    dinamic:dImageUrl="https://gw.alicdn.com/tfs/TB1ACgHuXkoBKNjSZFEXXbrEVXa-148-94.png"
                    dinamic:dMarginTop="8np"
                    dinamic:dWidth="match_parent"
                    dinamic:dHeight="24np"/>
                <TradeTextInput
                    dinamic:dText="@data{fields.quantity}"
                    dinamic:dTextSize="13np"
                    dinamic:dTextColor="#333333"
                    dinamic:dTextAlignment="center"
                    dinamic:dWidth="35np"
                    dinamic:dHeight="40np"
                    dinamic:dBackgroundColor="#00ffffff"
                    dinamic:dAccessibilityText="@concat{&apos;数量&apos;,@triple{@data{fields.quantity},@data{fields.quantity},&apos; &apos;}}"
                    dinamic:dAccessibilityTextHidden="false"
                    dinamic:onFinish="@handleDinamicXEvent{@const{changeQuantity},@const{change}}"
                    dinamic:dInputType="dialog"
                    dinamic:dTextGravity="center"
                    dinamic:dKeyboard="1"/>
            </DFrameLayout>
            <DImageView
                dinamic:dScaleType="fitCenter"
                dinamic:dImageUrl="@triple{@ilt{@data{fields.quantity},@data{fields.max}},@const{&apos;https://img.alicdn.com/tfs/TB145oLttcnBKNjSZR0XXcFqFXa-135-120.png&apos;},@const{&apos;https://img.alicdn.com/tfs/TB1bLfCtHsrBKNjSZFpXXcXhFXa-135-120.png&apos;}}"
                dinamic:dWidth="25np"
                dinamic:dHeight="25np"
                dinamic:dAccessibilityText="增加按钮"
                dinamic:dGravity="leftCenter"
                dinamic:onTap="@handleDinamicXEvent{@const{changeQuantity},@const{increase}}"/>
        </DLinearLayout>
    </DFrameLayout>
</DFrameLayout>
