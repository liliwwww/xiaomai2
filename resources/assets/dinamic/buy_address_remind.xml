<?xml version="1.0" encoding="utf-8"?>
<DFrameLayout xmlns:dinamic="http://schemas.android.com/apk/res-auto"
    dinamic:dOrientation="horizontal"
    dinamic:dWidth="375ap"
    dinamic:dHeight="40np"
    dinamic:dBackgroundColor="#ffffff"
    dinamic:dAccessibilityTextHidden="false"
    dinamic:onTap="@handleDinamicXEvent{@const{itemClick}}">
    <DLinearLayout
        dinamic:dOrientation="horizontal"
        dinamic:dWidth="match_content"
        dinamic:dHeight="match_content"
        dinamic:dGravity="leftCenter">
        <DImageView
            dinamic:dScaleType="centerInside"
            dinamic:dImageUrl="@data{fields.addressIcon}"
            dinamic:dMarginLeft="12np"
            dinamic:dWidth="24np"
            dinamic:dHeight="24np"
            dinamic:dGravity="center"/>
        <DLinearLayout
            dinamic:dOrientation="horizontal"
            dinamic:dWidth="305ap"
            dinamic:dHeight="match_content"
            dinamic:dGravity="leftCenter">
            <DTextView
                dinamic:dText="@data{fields.textSign}"
                dinamic:dTextSize="12np"
                dinamic:dTextColor="@theme{&apos;color&apos;, &apos;themeColor&apos;, &apos;#FF5000&apos;}"
                dinamic:dMaxLines="1"
                dinamic:dMarginLeft="8np"
                dinamic:dWidth="match_content"
                dinamic:dHeight="match_content"
                dinamic:dVisibility="@triple{@data{fields.textSign}, &apos;visible&apos;, &apos;gone&apos;}"
                dinamic:dAccessibilityTextHidden="false"
                dinamic:dFocusable="false"
                dinamic:dGravity="leftCenter"/>
            <DTextView
                dinamic:dText="@data{fields.addressDetail}"
                dinamic:dTextSize="12np"
                dinamic:dTextColor="#333333"
                dinamic:dMaxLines="1"
                dinamic:dLineBreakMode="end"
                dinamic:dMarginLeft="5np"
                dinamic:dWidth="0np"
                dinamic:dHeight="match_content"
                dinamic:dAccessibilityTextHidden="false"
                dinamic:dFocusable="false"
                dinamic:dGravity="leftCenter"
                dinamic:dWeight="1"/>
        </DLinearLayout>
    </DLinearLayout>
    <DLinearLayout
        dinamic:dOrientation="horizontal"
        dinamic:dWidth="match_content"
        dinamic:dHeight="match_content"
        dinamic:dGravity="rightCenter">
        <DImageView
            dinamic:dScaleType="fitCenter"
            dinamic:dMarginLeft="10np"
            dinamic:dMarginRight="10np"
            dinamic:dWidth="14np"
            dinamic:dHeight="14np"
            dinamic:dGravity="leftCenter"
            dinamic:dImageName="order_head_arrow"/>
    </DLinearLayout>
    <DView
        dinamic:dWidth="match_parent"
        dinamic:dHeight="0.5np"
        dinamic:dBackgroundColor="#cccccc"
        dinamic:dGravity="leftTop"/>
    <DView
        dinamic:dWidth="match_parent"
        dinamic:dHeight="0.5np"
        dinamic:dBackgroundColor="#cccccc"
        dinamic:dGravity="leftBottom"/>
</DFrameLayout>
