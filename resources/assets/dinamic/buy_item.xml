<?xml version="1.0" encoding="utf-8"?>
<DFrameLayout xmlns:dinamic="http://schemas.android.com/apk/res-auto"
    dinamic:dWidth="375ap"
    dinamic:dHeight="match_content"
    dinamic:dAccessibilityTextHidden="false">
    <DLinearLayout
        dinamic:dOrientation="vertical"
        dinamic:dMarginLeft="12np"
        dinamic:dMarginRight="12np"
        dinamic:dWidth="match_parent"
        dinamic:dHeight="match_content"
        dinamic:dBackgroundColor="#ffffffff"
        dinamic:dAccessibilityTextHidden="false"
        dinamic:dClipTopRightRadius="@triple{@or{@eq{@data{fields.cornerType}, &apos;top&apos;},@eq{@data{fields.cornerType}, &apos;both&apos;},&apos;false&apos;}, @theme{&apos;size&apos;, &apos;cornerRadius&apos;, &apos;12np&apos;},  @const{&apos;0np&apos;}}"
        dinamic:dClipTopLeftRadius="@triple{@or{@eq{@data{fields.cornerType}, &apos;top&apos;},@eq{@data{fields.cornerType}, &apos;both&apos;},&apos;false&apos;}, @theme{&apos;size&apos;, &apos;cornerRadius&apos;, &apos;12np&apos;},  @const{&apos;0np&apos;}}"
        dinamic:dClipBottomRightRadius="@triple{@or{@eq{@data{fields.cornerType}, &apos;bottom&apos;},@eq{@data{fields.cornerType}, &apos;both&apos;},&apos;false&apos;}, @theme{&apos;size&apos;, &apos;cornerRadius&apos;, &apos;12np&apos;},  @const{&apos;0np&apos;}}"
        dinamic:dClipBottomLeftRadius="@triple{@or{@eq{@data{fields.cornerType}, &apos;bottom&apos;},@eq{@data{fields.cornerType}, &apos;both&apos;},&apos;false&apos;}, @theme{&apos;size&apos;, &apos;cornerRadius&apos;, &apos;12np&apos;},  @const{&apos;0np&apos;}}">
        <DLinearLayout
            dinamic:dOrientation="horizontal"
            dinamic:dMarginLeft="12np"
            dinamic:dMarginRight="12np"
            dinamic:dMarginTop="6np"
            dinamic:dMarginBottom="12np"
            dinamic:dWidth="match_parent"
            dinamic:dHeight="match_content"
            dinamic:dAccessibilityTextHidden="false">
            <DFrameLayout
                dinamic:dMarginRight="12np"
                dinamic:dWidth="90np"
                dinamic:dHeight="90np"
                dinamic:dClipTopRightRadius="3np"
                dinamic:dClipTopLeftRadius="3np"
                dinamic:dClipBottomRightRadius="3np"
                dinamic:dClipBottomLeftRadius="3np">
                <DImageView
                    dinamic:dScaleType="fitCenter"
                    dinamic:dImageUrl="@data{fields.icon}"
                    dinamic:dWidth="match_parent"
                    dinamic:dHeight="match_parent"/>
                <DView
                    dinamic:dWidth="match_parent"
                    dinamic:dHeight="match_parent"
                    dinamic:dBackgroundColor="#05000000"/>
            </DFrameLayout>
            <DLinearLayout
                dinamic:dOrientation="vertical"
                dinamic:dWidth="match_parent"
                dinamic:dHeight="match_content">
                <DLinearLayout
                    dinamic:dOrientation="horizontal"
                    dinamic:dWidth="match_parent"
                    dinamic:dHeight="match_content">
                    <DLinearLayout
                        dinamic:dOrientation="vertical"
                        dinamic:dWidth="0np"
                        dinamic:dHeight="match_content"
                        dinamic:dWeight="1">
                        <DTextView
                            dinamic:dText="@data{fields.title}"
                            dinamic:dTextSize="12np"
                            dinamic:dTextColor="@triple{@data{fields.disabled},&apos;#ff999999&apos;, &apos;#ff333333&apos;}"
                            dinamic:dMaxLines="2"
                            dinamic:dLineBreakMode="end"
                            dinamic:dWidth="match_parent"
                            dinamic:dHeight="match_content"
                            dinamic:dAccessibilityTextHidden="false"
                            dinamic:dFocusable="false"/>
                        <DFrameLayout
                            dinamic:dWidth="match_parent"
                            dinamic:dHeight="match_content"
                            dinamic:dVisibility="@triple{@data{fields.subtitles}, &apos;visible&apos;, &apos;gone&apos;}">
                            <DTextView
                                dinamic:dText="@data{fields.subtitles}"
                                dinamic:dTextSize="12np"
                                dinamic:dTextColor="#FF9C9C9C"
                                dinamic:dMaxLines="0"
                                dinamic:dLineBreakMode="end"
                                dinamic:dMarginTop="6np"
                                dinamic:dMarginBottom="2np"
                                dinamic:dWidth="match_parent"
                                dinamic:dHeight="match_content"
                                dinamic:dVisibility="@triple{@data{fields.disabled}, &apos;visible&apos;, &apos;gone&apos;}"
                                dinamic:dAccessibilityTextHidden="false"
                                dinamic:dFocusable="false"
                                dinamic:dGravity="leftCenter"/>
                            <DLinearLayout
                                dinamic:dOrientation="vertical"
                                dinamic:dMarginTop="4np"
                                dinamic:dWidth="match_parent"
                                dinamic:dHeight="match_content">
                                <DLinearLayout
                                    dinamic:dOrientation="horizontal"
                                    dinamic:dWidth="match_parent"
                                    dinamic:dHeight="match_content"
                                    dinamic:dBackgroundColor="#fffafafa"
                                    dinamic:dVisibility="@triple{@data{fields.disabled}, &apos;gone&apos;, &apos;visible&apos;}"
                                    dinamic:dCornerRadius="3np">
                                    <DTextView
                                        dinamic:dText="@data{fields.subtitles}"
                                        dinamic:dTextSize="12np"
                                        dinamic:dTextColor="#FF9C9C9C"
                                        dinamic:dMaxLines="0"
                                        dinamic:dLineBreakMode="end"
                                        dinamic:dMarginLeft="3np"
                                        dinamic:dMarginRight="3np"
                                        dinamic:dMarginTop="2np"
                                        dinamic:dMarginBottom="2np"
                                        dinamic:dWidth="match_content"
                                        dinamic:dHeight="match_content"
                                        dinamic:dAccessibilityTextHidden="false"
                                        dinamic:dFocusable="false"
                                        dinamic:dGravity="leftCenter"
                                        dinamic:dRelayout="1"/>
                                    <DView
                                        dinamic:dWidth="0np"
                                        dinamic:dHeight="match_parent"
                                        dinamic:dBackgroundColor="#ffffffff"
                                        dinamic:dWeight="1"/>
                                </DLinearLayout>
                            </DLinearLayout>
                        </DFrameLayout>
                        <TradeRichTextView
                            dinamic:dTextSize="12np"
                            dinamic:dTextColor="#F5AC47"
                            dinamic:dMaxLines="0"
                            dinamic:dMarginTop="6np"
                            dinamic:dWidth="match_parent"
                            dinamic:dHeight="match_content"
                            dinamic:dVisibility="@triple{@data{fields.skuLevel}, &apos;visible&apos;, &apos;gone&apos;}"
                            dinamic:dAccessibilityTextHidden="false"
                            dinamic:dFocusable="false"
                            dinamic:dGravity="leftCenter"
                            dinamic:dRichText="@data{fields.skuLevel}"/>
                        <DLoopLinearLayout
                            dinamic:dOrientation="horizontal"
                            dinamic:dMarginTop="7np"
                            dinamic:dWidth="match_parent"
                            dinamic:dHeight="match_content"
                            dinamic:dVisibility="@triple{@and{@not{@data{fields.disabled}}, @triple{@platform{&apos;Android&apos;}, @triple{@igt{@len{@data{fields.icons}}, &apos;0&apos;}, &apos;true&apos;, &apos;false&apos;}, @data{fields.icons}}}, &apos;visible&apos;,&apos;gone&apos;}"
                            dinamic:dListData="@data{fields.icons}">
                            <DImageView
                                dinamic:dScaleType="fitCenter"
                                dinamic:dImageUrl="@subdata{value}"
                                dinamic:dMarginRight="3np"
                                dinamic:dWidth="match_content"
                                dinamic:dHeight="14np"
                                dinamic:dGravity="leftCenter"
                                dinamic:onComplete="@imageRefresh{}"/>
                        </DLoopLinearLayout>
                        <DLoopLinearLayout
                            dinamic:dOrientation="horizontal"
                            dinamic:dMarginTop="8np"
                            dinamic:dWidth="match_parent"
                            dinamic:dHeight="match_content"
                            dinamic:dVisibility="@triple{@and{@not{@data{fields.disabled}}, @triple{@platform{&apos;Android&apos;}, @triple{@igt{@len{@data{fields.services}}, &apos;0&apos;}, &apos;true&apos;, &apos;false&apos;}, @data{fields.services}}}, &apos;visible&apos;, &apos;gone&apos;}"
                            dinamic:dListData="@data{fields.services}">
                            <DFrameLayout
                                dinamic:dMarginRight="3np"
                                dinamic:dWidth="match_content"
                                dinamic:dHeight="match_content"
                                dinamic:dBackgroundColor="@subdata{bgColor}; @const{&apos;#FFFFF1EB&apos;}"
                                dinamic:dCornerRadius="2np">
                                <DTextView
                                    dinamic:dText="@subdata{value}"
                                    dinamic:dTextSize="10np"
                                    dinamic:dTextColor="@subdata{borderColor}; @const{&apos;#FFFF5000&apos;}"
                                    dinamic:dMaxLines="1"
                                    dinamic:dLineBreakMode="end"
                                    dinamic:dMarginLeft="3np"
                                    dinamic:dMarginRight="3np"
                                    dinamic:dWidth="match_content"
                                    dinamic:dHeight="match_content"
                                    dinamic:dAccessibilityTextHidden="false"
                                    dinamic:dFocusable="false"
                                    dinamic:dGravity="center"/>
                            </DFrameLayout>
                        </DLoopLinearLayout>
                    </DLinearLayout>
                    <DLinearLayout
                        dinamic:dOrientation="vertical"
                        dinamic:dMarginLeft="12np"
                        dinamic:dWidth="match_content"
                        dinamic:dHeight="match_content">
                        <DLinearLayout
                            dinamic:dOrientation="horizontal"
                            dinamic:dWidth="match_content"
                            dinamic:dHeight="match_content">
                            <TradePriceView
                                dinamic:dTextSize="13np"
                                dinamic:dTextColor="#ff333333"
                                dinamic:dMaxLines="2"
                                dinamic:dLineBreakMode="end"
                                dinamic:dMaxWidth="110np"
                                dinamic:dWidth="match_content"
                                dinamic:dHeight="match_content"
                                dinamic:dAccessibilityTextHidden="false"
                                dinamic:dFocusable="false"
                                dinamic:dGravity="rightTop"
                                dinamic:dPrice="@data{fields.price}"
                                dinamic:dSymbolScale="0.77"
                                dinamic:dDecimalScale="0.77"/>
                            <DTextView
                                dinamic:dText="@data{fields.timeLimit}"
                                dinamic:dTextSize="12np"
                                dinamic:dTextColor="#ff333333"
                                dinamic:dMaxLines="1"
                                dinamic:dLineBreakMode="end"
                                dinamic:dMaxWidth="20np"
                                dinamic:dWidth="match_content"
                                dinamic:dHeight="match_content"
                                dinamic:dVisibility="@triple{@data{fields.timeLimit}, &apos;visible&apos;, &apos;gone&apos;}"
                                dinamic:dAccessibilityTextHidden="false"
                                dinamic:dFocusable="false"
                                dinamic:dGravity="rightCenter"/>
                        </DLinearLayout>
                        <DTextView
                            dinamic:dText="@data{fields.count}"
                            dinamic:dTextSize="12np"
                            dinamic:dTextColor="#ff999999"
                            dinamic:dMaxLines="1"
                            dinamic:dLineBreakMode="end"
                            dinamic:dMaxWidth="110np"
                            dinamic:dWidth="match_content"
                            dinamic:dHeight="match_content"
                            dinamic:dAccessibilityTextHidden="false"
                            dinamic:dFocusable="false"
                            dinamic:dGravity="rightTop"/>
                        <DTextView
                            dinamic:dText="@data{fields.weight}"
                            dinamic:dTextSize="12np"
                            dinamic:dTextColor="#ff999999"
                            dinamic:dMaxLines="1"
                            dinamic:dLineBreakMode="end"
                            dinamic:dMaxWidth="110np"
                            dinamic:dWidth="match_content"
                            dinamic:dHeight="match_content"
                            dinamic:dAccessibilityTextHidden="false"
                            dinamic:dFocusable="false"
                            dinamic:dGravity="rightTop"/>
                    </DLinearLayout>
                </DLinearLayout>
                <DLinearLayout
                    dinamic:dOrientation="horizontal"
                    dinamic:dMarginTop="8np"
                    dinamic:dWidth="match_parent"
                    dinamic:dHeight="match_content"
                    dinamic:dVisibility="@triple{@data{fields.disabled}, &apos;visible&apos;, &apos;gone&apos;}">
                    <DTextView
                        dinamic:dText="@data{fields.desc}"
                        dinamic:dTextSize="12np"
                        dinamic:dTextColor="@theme{&apos;color&apos;, &apos;themeColor&apos;, &apos;#FFFF5000&apos;}"
                        dinamic:dMaxLines="2"
                        dinamic:dLineBreakMode="end"
                        dinamic:dWidth="0np"
                        dinamic:dHeight="match_content"
                        dinamic:dAccessibilityTextHidden="false"
                        dinamic:dFocusable="false"
                        dinamic:dGravity="leftCenter"
                        dinamic:dWeight="1"/>
                </DLinearLayout>
            </DLinearLayout>
        </DLinearLayout>
    </DLinearLayout>
</DFrameLayout>
