<?xml version="1.0" encoding="utf-8"?>
<DLinearLayout xmlns:dinamic="http://schemas.android.com/apk/res-auto"
    dinamic:dOrientation="horizontal"
    dinamic:dWidth="375ap"
    dinamic:dHeight="match_content"
    dinamic:dAccessibilityTextHidden="false"
    dinamic:onTap="@handleDinamicXEvent{&apos;popupSelect&apos;}">
    <DLinearLayout
        dinamic:dOrientation="vertical"
        dinamic:dMarginLeft="12np"
        dinamic:dMarginTop="12np"
        dinamic:dMarginBottom="12np"
        dinamic:dWidth="0np"
        dinamic:dHeight="match_content"
        dinamic:dGravity="leftCenter"
        dinamic:dWeight="1.0">
        <DTextView
            dinamic:dText="@data{fields.title}"
            dinamic:dTextSize="14np"
            dinamic:dTextColor="@triple{@data{fields.titleCss.color},@data{fields.titleCss.color},&apos;#333333&apos;}"
            dinamic:dMaxLines="0"
            dinamic:dMaxWidth="270np"
            dinamic:dWidth="match_content"
            dinamic:dHeight="match_content"
            dinamic:dAccessibilityTextHidden="false"
            dinamic:dFocusable="false"
            dinamic:dGravity="leftCenter"/>
        <DTextView
            dinamic:dText="@data{fields.subTitle}"
            dinamic:dTextSize="@triple{@data{fields.subTitleCss.font}, @data{fields.subTitleCss.font} ,&apos;14np&apos;}"
            dinamic:dTextColor="@triple{@data{fields.subTitleCss.color},@data{fields.subTitleCss.color},&apos;#333333&apos;}"
            dinamic:dMaxLines="1"
            dinamic:dLineBreakMode="end"
            dinamic:dMaxWidth="270np"
            dinamic:dMarginTop="4np"
            dinamic:dWidth="match_content"
            dinamic:dHeight="match_content"
            dinamic:dVisibility="@triple{@data{fields.subTitle}, &apos;visible&apos; ,&apos;gone&apos;}"
            dinamic:dAccessibilityTextHidden="false"
            dinamic:dFocusable="false"
            dinamic:dGravity="leftCenter"/>
    </DLinearLayout>
    <DLinearLayout
        dinamic:dOrientation="horizontal"
        dinamic:dMarginLeft="11np"
        dinamic:dMarginRight="12np"
        dinamic:dMarginTop="15np"
        dinamic:dMarginBottom="15np"
        dinamic:dWidth="match_content"
        dinamic:dHeight="match_content"
        dinamic:dGravity="rightCenter">
        <DTextView
            dinamic:dText="@data{fields.price}"
            dinamic:dTextSize="14np"
            dinamic:dTextColor="@triple{@data{fields.priceCss.color},@data{fields.priceCss.color},&apos;#333333&apos;}"
            dinamic:dMaxLines="0"
            dinamic:dMarginRight="8np"
            dinamic:dWidth="match_content"
            dinamic:dHeight="match_content"
            dinamic:dAccessibilityTextHidden="false"
            dinamic:dFocusable="false"
            dinamic:dGravity="leftCenter"/>
        <DCheckBox
            dinamic:dMarginLeft="3np"
            dinamic:dWidth="17np"
            dinamic:dHeight="17np"
            dinamic:dVisibility="@triple{@data{fields.checkboxVisible}, @data{fields.checkboxVisible}, &apos;visible&apos;}"
            dinamic:dGravity="rightCenter"
            dinamic:dChecked="@data{fields.isChecked}"
            dinamic:dCheckImg="icon_checked"
            dinamic:dUncheckImg="icon_unchecked"
            dinamic:dEnabled="@triple{@data{fields.disabled},&apos;0&apos;,&apos;1&apos;}"
            dinamic:onChange="@handleDinamicXEvent{&apos;popupSelect&apos;}"
            dinamic:tScaleType="fitCenter"/>
    </DLinearLayout>
</DLinearLayout>
