{
  "data": {
    "store_image": {
      "value": "@{eval(size(${storeIcon}) > 0) ? ${storeIcon} : @{env(isAndroid) ? 'gaiax_res:bricks_coupon_store' : 'gaiax_res:coupon_info_store_img' }}"
    },
    "store_name": {
      "extend": {
        "fit-content": "false",
        "font-weight": "@{env(isAndroid) ? '400' : '600'}"
      },
      "value": "${storeName}"
    },
    "coupon_info": {
