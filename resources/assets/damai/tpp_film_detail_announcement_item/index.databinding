    "right_arrow": {
      "extend": { "font-family": "@{env(isAndroid) ? icon : iconfont2 }" },
      "value": "&#xe8d3"
    },
    "title": { "value": "${name}", "extend": { "fit-content": "false" } },
    "sub_title": { "value": "${title}", "extend": { "fit-content": "false" } },
